<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"        android:layout_width="match_parent"        android:layout_height="match_parent"        android:background="#efefef"        android:fitsSystemWindows="true"        android:orientation="vertical">    <LinearLayout            android:id="@+id/systempaymentIntegralPayBody"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_marginLeft="20dp"            android:layout_marginTop="40dp"            android:layout_marginRight="20dp"            android:background="#ffffff"            android:orientation="vertical"            android:paddingTop="10dp"            android:paddingBottom="10dp">        <RelativeLayout                android:layout_width="match_parent"                android:layout_height="70dp">            <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginLeft="10dp"                    android:layout_marginTop="10dp"                    android:text="支付积分给"                    android:textColor="#666666"                    android:textSize="9dp" />            <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_alignParentBottom="true"                    android:layout_marginLeft="10dp"                    android:layout_marginTop="25dp"                    android:layout_marginBottom="20dp"                    android:text="龙岩市左边网络科技有限公司"                    android:textColor="#000000"                    android:textSize="15dp" />            <ImageView                    android:layout_width="70dp"                    android:layout_height="70dp"                    android:layout_alignParentRight="true"                    android:src="@drawable/ico_company" />        </RelativeLayout>        <View                android:layout_width="match_parent"                android:layout_height="2dp"                android:background="@drawable/border_dotted_line"                android:layerType="software" />        <RelativeLayout                android:layout_width="match_parent"                android:layout_height="wrap_content">            <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginLeft="20dp"                    android:layout_marginTop="10dp"                    android:text="提现金额"                    android:textColor="#000000"                    android:textSize="10dp" />        </RelativeLayout>        <!--提现的输入框-->        <RelativeLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="10dp">            <!--提现的金额的符号-->            <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                    android:id="@+id/activity_integraldepositSymbol"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_centerVertical="true"                    android:layout_marginLeft="20dp"                    android:text="@string/PriceSymbol"                    android:textColor="#000000"                    android:textSize="15dp"                    android:textStyle="bold" />            <!--提现的金额-->            <EditText                    android:id="@+id/activity_integralPaymentEditMoney"                    android:layout_width="150dp"                    android:layout_height="30dp"                    android:layout_centerVertical="true"                    android:layout_toRightOf="@id/activity_integraldepositSymbol"                    android:background="@null"                    android:inputType="number"                    android:maxLength="5"                    android:paddingLeft="5dp"                    android:textColor="@color/colorText"                    android:textSize="25dp"                    android:textStyle="bold" />        </RelativeLayout>        <!--下划线-->        <View                android:layout_width="match_parent"                android:layout_height="2dp"                android:layout_marginLeft="15dp"                android:layout_marginTop="10dp"                android:layout_marginRight="15dp"                android:background="@drawable/border_dotted_line"                android:layerType="software" />        <!--超出提现的金额-->        <RelativeLayout                android:layout_width="match_parent"                android:layout_height="wrap_content">            <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                    android:id="@+id/activity_integraldepositIntegralmsg"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_alignParentLeft="true"                    android:paddingLeft="15dp"                    android:paddingTop="8dp"                    android:paddingBottom="8dp"                    android:textColor="@color/colorText"                    android:textSize="10dp" />            <ImageView                    android:id="@+id/activity_integraldepositBtnAsk"                    android:layout_width="18dp"                    android:layout_height="18dp"                    android:layout_centerVertical="true"                    android:layout_marginLeft="5dp"                    android:layout_toRightOf="@id/activity_integraldepositIntegralmsg"                    android:src="@drawable/ico_ask" />        </RelativeLayout>        <!--点击支付-->        <RelativeLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="15dp">            <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                    android:id="@+id/systempaymentIntegralBtntopay"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_centerHorizontal="true"                    android:gravity="center"                    android:paddingLeft="30dp"                    android:paddingTop="7dp"                    android:paddingRight="30dp"                    android:paddingBottom="7dp"                    android:text="确定支付"                    android:textSize="10dp" />        </RelativeLayout>    </LinearLayout>    <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentBottom="true"            android:layout_centerHorizontal="true"            android:layout_marginBottom="10dp"            android:text="@string/SystemBottommsg"            android:textColor="#afafaf"            android:textSize="10dp" /></RelativeLayout>