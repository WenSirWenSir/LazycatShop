<?xml version="1.0" encoding="utf-8"?><!--展示用户提交的发货单--><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:orientation="vertical">    <RelativeLayout        android:layout_width="match_parent"        android:layout_height="20dp">        <TextView            android:id="@+id/item_sendsystemOrderact_list_orderNumberTitle"            android:layout_width="wrap_content"            android:layout_height="match_parent"            android:layout_centerVertical="true"            android:layout_marginLeft="5dp"            android:gravity="center_vertical"            android:text="发货单号:"            android:textColor="#020433"            android:textSize="9sp" />        <shlm.lmcs.com.lazycat.LazyShopView.PriceNumberView            android:layout_width="wrap_content"            android:layout_height="match_parent"            android:id="@+id/item_sendsystemorderact_list_orderNumber"            android:layout_centerVertical="true"            android:layout_toRightOf="@id/item_sendsystemOrderact_list_orderNumberTitle"            android:gravity="center_vertical"            android:paddingBottom="1dp"            android:text="089385938"            android:textColor="#020433"            android:textSize="10sp" />        <TextView            android:layout_width="wrap_content"            android:layout_height="match_parent"            android:layout_alignParentRight="true"            android:layout_centerVertical="true"            android:background="@color/ThemeColor"            android:id="@+id/item_sendsystemorderact_list_status"            android:gravity="center_vertical"            android:paddingLeft="10dp"            android:paddingRight="10dp"            android:text="发货单已提交"            android:textColor="#ffffff"            android:textSize="9sp" />    </RelativeLayout>    <LinearLayout        android:layout_width="match_parent"        android:layout_height="110dp">        <RelativeLayout            android:layout_width="110dp"            android:layout_height="110dp">            <LinearLayout                android:layout_width="100dp"                android:layout_height="100dp"                android:layout_centerInParent="true">            </LinearLayout>        </RelativeLayout>        <RelativeLayout            android:layout_width="match_parent"            android:layout_height="match_parent">            <LinearLayout                android:layout_width="match_parent"                android:layout_height="match_parent"                android:orientation="vertical">                <TextView                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="5dp"                    android:layout_marginRight="10dp"                    android:ellipsize="end"                    android:maxLines="1"                    android:text="海天750ML味极鲜酱油海天750ML味极鲜酱油海天750ML味极鲜酱油"                    android:textColor="#020433"                    android:textSize="12sp" />                <TextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginTop="5dp"                    android:layout_marginRight="10dp"                    android:ellipsize="end"                    android:id="@+id/item_sendsystemorderact_list_spec"                    android:maxLines="1"                    android:textColor="#333333"                    android:textSize="10sp" />                <TextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginTop="5dp"                    android:layout_marginRight="10dp"                    android:ellipsize="end"                    android:maxLines="1"                    android:id="@+id/item_sendsystemorderact_list_expandpd"                    android:textColor="#333333"                    android:textSize="10sp" />                <TextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginTop="5dp"                    android:layout_marginRight="10dp"                    android:ellipsize="end"                    android:id="@+id/item_sendsystemorderact_list_sendTopay"                    android:maxLines="1"                    android:text="订购数量:1箱"                    android:textColor="#333333"                    android:textSize="10sp" />                <TextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginTop="5dp"                    android:layout_marginRight="10dp"                    android:id="@+id/item_sendsystemorderact_list_createTime"                    android:ellipsize="end"                    android:maxLines="1"                    android:textColor="#333333"                    android:textSize="10sp" />            </LinearLayout>            <!--等待支付的价格-->            <LinearLayout                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_alignParentRight="true"                android:layout_alignParentBottom="true"                android:gravity="right"                android:paddingRight="20dp">                <TextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:text="等待支付"                    android:textSize="9sp" />                <TextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:text="@string/PriceSymbol"                    android:textSize="9sp" />                <shlm.lmcs.com.lazycat.LazyShopView.PriceNumberView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:text="19099"                    android:textColor="@color/colorPrice"                    android:id="@+id/item_sendsystemorderact_list_tp"                    android:textSize="17sp" />            </LinearLayout>        </RelativeLayout>    </LinearLayout>    <!--无货 通知用户-->    <RelativeLayout        android:layout_width="match_parent"        android:layout_height="20dp"        android:layout_marginTop="5dp"        >        <!--点击通知取消-->        <TextView            android:layout_width="80dp"            android:layout_height="20dp"            android:textSize="9sp"            android:text="我已知晓"            android:layout_alignParentRight="true"            android:textColor="#ffffff"            android:background="@color/ThemeColor"            android:gravity="center"            />    </RelativeLayout>    <View        android:layout_width="match_parent"        android:layout_height="2dp"        android:layout_marginTop="5dp"        android:layout_marginRight="82dp"        android:background="@drawable/border_dotted_line"        android:layerType="software" /></LinearLayout>