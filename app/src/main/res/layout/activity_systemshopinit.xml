<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"        android:layout_width="match_parent"        android:layout_height="match_parent"        android:background="#efefef"        android:fitsSystemWindows="true"        android:orientation="vertical">    <include layout="@layout/assembly_act_head" />    <!--要求输出的文字信息-->    <RelativeLayout            android:layout_width="match_parent"            android:layout_height="match_parent">        <LinearLayout                android:id="@+id/activity_systemshopinitValuesBody"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_centerHorizontal="true"                android:layout_marginLeft="10dp"                android:layout_marginTop="50dp"                android:layout_marginRight="10dp"                android:background="#ffffff"                android:orientation="vertical"                android:paddingTop="20dp"                android:paddingBottom="20dp">            <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:orientation="horizontal">                <RelativeLayout                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="1">                    <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_alignParentRight="true"                            android:layout_centerVertical="true"                            android:text="输入标题:"                            android:textColor="#a9a9a9"                            android:textSize="12dp" />                </RelativeLayout>                <RelativeLayout                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="2">                    <shlm.lmcs.com.lazycat.LazyShopView.SystemEditText                            android:id="@+id/activity_systemshopinitInputTitle"                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:background="@null"                            android:paddingLeft="10dp"                            android:textSize="12dp" />                </RelativeLayout>            </LinearLayout>            <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="10dp"                    android:orientation="horizontal">                <RelativeLayout                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="1">                    <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_alignParentRight="true"                            android:layout_centerVertical="true"                            android:text="输入条码:"                            android:textColor="#a9a9a9"                            android:textSize="12dp" />                </RelativeLayout>                <RelativeLayout                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="2">                    <shlm.lmcs.com.lazycat.LazyShopView.SystemEditText                            android:id="@+id/activity_systemshopinitInputBarcode"                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:background="@null"                            android:inputType="number"                            android:paddingLeft="10dp"                            android:textSize="12dp" />                </RelativeLayout>            </LinearLayout>            <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="10dp"                    android:orientation="horizontal">                <RelativeLayout                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="1">                    <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_alignParentRight="true"                            android:layout_centerVertical="true"                            android:text="输入批发价格:"                            android:textColor="#a9a9a9"                            android:textSize="12dp" />                </RelativeLayout>                <RelativeLayout                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="2">                    <shlm.lmcs.com.lazycat.LazyShopView.SystemEditText                            android:id="@+id/activity_systemshopinitInputTp"                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:background="@null"                            android:inputType="number"                            android:paddingLeft="10dp"                            android:textSize="12dp" />                </RelativeLayout>            </LinearLayout>            <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="10dp"                    android:orientation="horizontal">                <RelativeLayout                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="1">                    <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_alignParentRight="true"                            android:layout_centerVertical="true"                            android:text="建议零售价:"                            android:textColor="#a9a9a9"                            android:textSize="12dp" />                </RelativeLayout>                <RelativeLayout                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="2">                    <shlm.lmcs.com.lazycat.LazyShopView.SystemEditText                            android:id="@+id/activity_systemshopinitInputRetail"                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:background="@null"                            android:inputType="number"                            android:paddingLeft="10dp"                            android:textSize="12dp" />                </RelativeLayout>            </LinearLayout>            <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="10dp"                    android:orientation="horizontal">                <RelativeLayout                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="1">                    <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_alignParentRight="true"                            android:layout_centerVertical="true"                            android:text="品牌:"                            android:textColor="#a9a9a9"                            android:textSize="12dp" />                </RelativeLayout>                <RelativeLayout                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="2">                    <shlm.lmcs.com.lazycat.LazyShopView.SystemEditText                            android:id="@+id/activity_systemshopinitInputBrand"                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:background="@null"                            android:paddingLeft="10dp"                            android:textSize="12dp" />                </RelativeLayout>            </LinearLayout>            <RelativeLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="10dp">                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                        android:id="@+id/activity_systemshopinitBtnInit"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_centerInParent="true"                        android:paddingLeft="10dp"                        android:paddingTop="5dp"                        android:paddingRight="10dp"                        android:paddingBottom="5dp"                        android:text="商品初始化入库"                        android:textColor="#ffffff"                        android:textSize="12dp" />            </RelativeLayout>        </LinearLayout>    </RelativeLayout></LinearLayout>