<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"        xmlns:app="http://schemas.android.com/apk/res-auto"        android:layout_width="match_parent"        android:layout_height="match_parent"        android:background="#ffffff"        android:orientation="vertical">    <LinearLayout            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_centerHorizontal="true"            android:layout_marginTop="100dp"            android:layout_weight="2"            android:background="#ffffff"            android:gravity="center"            android:orientation="vertical">        <TextView                android:id="@+id/activity_lazy_log_title"                android:layout_width="80dp"                android:layout_height="80dp"                android:gravity="center"                android:text="仓"                android:textColor="#DB0025"                android:textSize="70dp" />        <TextView                android:id="@+id/activity_lazy_log_context"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginTop="15dp"                android:text="CangKu Service"                android:textColor="#020433"                android:textSize="18sp" />        <TextView                android:id="@+id/activity_lazy_cat_log_Propaganda"                android:layout_width="290dp"                android:layout_height="wrap_content"                android:layout_marginTop="5dp"                android:gravity="center"                android:text="更加便携的仓储服务配送商城,让您足不出户"                android:textColor="#000000"                android:textSize="14dp" />        <TextView                android:id="@+id/activity_lazy_cat_log_Propaganda_b"                android:layout_width="250dp"                android:layout_height="wrap_content"                android:gravity="center"                android:text="享受便携的配送体验"                android:textColor="#000000"                android:textSize="14dp" />    </LinearLayout></RelativeLayout>