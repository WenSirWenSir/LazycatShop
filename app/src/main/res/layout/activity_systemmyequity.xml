<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"        android:layout_width="match_parent"        android:layout_height="match_parent"        android:background="#efefef"        android:fitsSystemWindows="true"        android:orientation="vertical">    <include layout="@layout/assembly_act_head" />    <RelativeLayout            android:layout_width="match_parent"            android:layout_height="match_parent">        <!--做一个卡片样式-->        <RelativeLayout                android:id="@+id/activity_systemmyequity_carbody"                android:layout_width="match_parent"                android:layout_height="200dp"                android:layout_margin="10dp"                android:background="#a9a9a9">            <ImageView                    android:id="@+id/activity_systemmyequity_Vipimg"                    android:layout_width="65dp"                    android:layout_height="65dp"                    android:layout_marginLeft="20dp"                    android:layout_marginTop="10dp"                    android:src="@drawable/ico_myequity"                    android:visibility="gone" />            <!--圆形按钮白色-->            <View                    android:id="@+id/activity_systemmyequity_Roundview"                    android:layout_width="18dp"                    android:layout_height="18dp"                    android:layout_alignParentRight="true"                    android:layout_marginTop="20dp"                    android:layout_marginRight="10dp"                    android:background="#ffffff" />            <!--个体户名称-->            <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                    android:id="@+id/activity_systemmyequity_businessname"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginTop="22dp"                    android:layout_marginRight="10dp"                    android:layout_toLeftOf="@id/activity_systemmyequity_Roundview"                    android:text="龙岩市左边网络科技有限公司"                    android:textColor="#ffffff"                    android:textSize="15dp" />            <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_below="@id/activity_systemmyequity_businessname"                    android:layout_marginTop="10dp"                    android:layout_marginRight="10dp"                    android:layout_toLeftOf="@id/activity_systemmyequity_Roundview"                    android:text="翁启鑫"                    android:textColor="#ffffff"                    android:textSize="15dp" />            <!--售后的内容-->            <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                    android:id="@+id/activity_systemmyequity_equity"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_alignParentBottom="true"                    android:layout_centerHorizontal="true"                    android:layout_marginBottom="70dp"                    android:textColor="#ffffff"                    android:textSize="17dp" />            <!--权益是否生效-->            <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                    android:id="@+id/activity_systemmyequity_TimeOver"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_alignParentRight="true"                    android:layout_alignParentBottom="true"                    android:layout_marginRight="10dp"                    android:layout_marginBottom="10dp"                    android:text="权益到期日:2020-01-20 20:20:20"                    android:textColor="#ffffff"                    android:textSize="11dp"                    android:visibility="gone" />        </RelativeLayout>        <!--设置权益说明-->        <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_below="@id/activity_systemmyequity_carbody"                android:layout_marginLeft="10dp"                android:layout_marginTop="25dp"                android:layout_marginRight="10dp"                android:orientation="vertical">            <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:text="权益说明"                    android:textColor="#000000"                    android:textSize="18dp" />            <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="10dp"                    android:orientation="horizontal">                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:text="权益说明:"                        android:textColor="#000000"                        android:textSize="10dp" />                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:text="从加盟秒开始至加盟结束秒都具有商品的维护特权。即使您在没有加盟权限下,您在有权限时购买的货物都具备维护的特权。在云仓库的所有购买的货物都具备售后服务"                        android:textColor="#a9a9a9"                        android:textSize="10dp" />            </LinearLayout>            <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="10dp"                    android:orientation="horizontal">                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:text="积分生效:"                        android:textColor="#000000"                        android:textSize="10dp" />                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:text="从加盟秒开始至加盟结束秒都具有商品的积分生效特权,积分可以进行提现操作。"                        android:textColor="#a9a9a9"                        android:textSize="10dp" />            </LinearLayout>            <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="10dp"                    android:orientation="horizontal">                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:text="优先配送:"                        android:textColor="#000000"                        android:textSize="10dp" />                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:text="从加盟秒开始至加盟结束秒都具有商品的维护特权。您在云仓库购买的货物将进行快速配货/送货。"                        android:textColor="#a9a9a9"                        android:textSize="10dp" />            </LinearLayout>            <RelativeLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="25dp">                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_alignParentRight="true"                        android:text="龙岩市左边网络科技有限公司保留所有的加盟解释权"                        android:textColor="@color/ThemeColor"                        android:textSize="10dp" />            </RelativeLayout>        </LinearLayout>        <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_alignParentBottom="true"                android:layout_centerHorizontal="true"                android:layout_marginBottom="5dp"                android:text="@string/SystemBottommsg"                android:textColor="#a9a9a9"                android:textSize="10dp" />    </RelativeLayout></LinearLayout>