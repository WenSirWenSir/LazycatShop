<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="#efefef"    android:fitsSystemWindows="true"    android:orientation="vertical">    <!--头部显示个人的信息-->    <ScrollView        android:layout_width="match_parent"        android:layout_height="match_parent"        android:scrollbars="none">        <LinearLayout            android:layout_width="match_parent"            android:layout_height="match_parent"            android:background="#efefef"            android:orientation="vertical">            <!--显示余额-->            <!--显示ITEM-->            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:orientation="vertical">                <RelativeLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="30dp"                    android:background="#efefef"                    android:paddingBottom="10dp">                    <RelativeLayout                        android:layout_width="match_parent"                        android:layout_height="55dp">                        <!--做一个标题-->                        <LinearLayout                            android:layout_width="wrap_content"                            android:layout_height="match_parent">                            <TextView                                android:id="@+id/fragment_user_ico"                                android:layout_width="wrap_content"                                android:layout_height="match_parent"                                android:layout_marginLeft="10dp"                                android:gravity="center"                                android:text="仓"                                android:textColor="@color/ThemeColor"                                android:textSize="35dp" />                            <TextView                                android:id="@+id/fragment_user_icoTitle"                                android:layout_width="wrap_content"                                android:layout_height="match_parent"                                android:layout_marginLeft="5dp"                                android:gravity="center"                                android:text="Cangku  Service"                                android:textColor="@color/colorText"                                android:textSize="25dp" />                        </LinearLayout>                        <ImageView                            android:id="@+id/fragment_user_btnExit"                            android:layout_width="35dp"                            android:layout_height="100dp"                            android:layout_alignParentRight="true"                            android:layout_centerInParent="true"                            android:layout_marginTop="30dp"                            android:layout_marginRight="10dp"                            android:src="@drawable/ico_exit" />                    </RelativeLayout>                    <LinearLayout                        android:id="@+id/usercenterfrg_userValuesBody"                        android:layout_width="match_parent"                        android:layout_height="130dp"                        android:layout_marginLeft="10dp"                        android:layout_marginTop="90dp"                        android:layout_marginRight="10dp"                        android:background="#ffffff"                        android:orientation="vertical">                        <RelativeLayout                            android:layout_width="match_parent"                            android:layout_height="80dp">                            <RelativeLayout                                android:layout_width="wrap_content"                                android:layout_height="40dp"                                android:layout_centerVertical="true"                                android:layout_marginLeft="20dp"                                android:orientation="horizontal">                                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                                    android:id="@+id/fragmen_user_business_name"                                    android:layout_width="wrap_content"                                    android:layout_height="wrap_content"                                    android:layout_marginLeft="10dp"                                    android:textColor="#000000"                                    android:textSize="15sp"                                    android:textStyle="bold" />                                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                                    android:id="@+id/fragment_user_vip"                                    android:layout_width="wrap_content"                                    android:layout_height="wrap_content"                                    android:layout_marginLeft="10dp"                                    android:layout_toRightOf="@id/fragmen_user_business_name"                                    android:gravity="center"                                    android:paddingLeft="10dp"                                    android:paddingTop="2dp"                                    android:paddingRight="10dp"                                    android:paddingBottom="2dp"                                    android:textColor="#666666"                                    android:textSize="8sp"                                    android:textStyle="bold" />                                <shlm.lmcs.com.lazycat.LazyShopView.PriceNumberView                                    android:id="@+id/fragment_user_TvLongLat"                                    android:layout_width="wrap_content"                                    android:layout_height="wrap_content"                                    android:layout_alignParentBottom="true"                                    android:layout_marginLeft="10dp"                                    android:textSize="10sp" />                            </RelativeLayout>                        </RelativeLayout>                        <LinearLayout                            android:layout_width="match_parent"                            android:layout_height="50dp"                            android:layout_alignParentBottom="true"                            android:orientation="horizontal">                            <RelativeLayout                                android:id="@+id/fragment_user_btnInteger"                                android:layout_width="0dp"                                android:layout_height="match_parent"                                android:layout_weight="1"                                android:orientation="vertical">                                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                                    android:layout_width="match_parent"                                    android:layout_height="wrap_content"                                    android:gravity="center"                                    android:text="加盟积分"                                    android:textColor="@color/colorText"                                    android:textSize="10sp" />                                <LinearLayout                                    android:layout_width="wrap_content"                                    android:layout_height="wrap_content"                                    android:layout_centerInParent="true"                                    android:orientation="horizontal">                                    <shlm.lmcs.com.lazycat.LazyShopView.PriceNumberView                                        android:id="@+id/fragment_user_integral"                                        android:layout_width="wrap_content"                                        android:layout_height="wrap_content"                                        android:textColor="@color/colorVip"                                        android:textSize="15sp" />                                </LinearLayout>                            </RelativeLayout>                            <RelativeLayout                                android:id="@+id/fragment_user_BtnpayStatusRecord"                                android:layout_width="0dp"                                android:layout_height="match_parent"                                android:layout_weight="1"                                android:orientation="vertical">                                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                                    android:layout_width="match_parent"                                    android:layout_height="wrap_content"                                    android:gravity="center"                                    android:text="Vip充值记录"                                    android:textColor="@color/colorText"                                    android:textSize="10sp" />                                <shlm.lmcs.com.lazycat.LazyShopView.PriceNumberView                                    android:layout_width="wrap_content"                                    android:layout_height="wrap_content"                                    android:layout_centerInParent="true"                                    android:text="0"                                    android:textColor="#000000"                                    android:textSize="15sp" />                            </RelativeLayout>                            <RelativeLayout                                android:layout_width="0dp"                                android:layout_height="match_parent"                                android:layout_weight="1">                                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                                    android:layout_width="match_parent"                                    android:layout_height="wrap_content"                                    android:gravity="center"                                    android:text="Vip到期时间"                                    android:textColor="@color/colorText"                                    android:textSize="10sp" />                                <shlm.lmcs.com.lazycat.LazyShopView.PriceNumberView                                    android:id="@+id/fragment_user_UservipDone"                                    android:layout_width="wrap_content"                                    android:layout_height="wrap_content"                                    android:layout_centerInParent="true"                                    android:textColor="#f30d65"                                    android:textSize="10dp"                                    />                            </RelativeLayout>                        </LinearLayout>                    </LinearLayout>                </RelativeLayout>                <!--物流信息的导航-->                <LinearLayout                    android:id="@+id/fragment_user_orderNav"                    android:layout_width="match_parent"                    android:layout_height="100dp"                    android:layout_marginLeft="10dp"                    android:layout_marginRight="10dp"                    android:background="#ffffff"                    android:orientation="vertical">                    <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginLeft="10dp"                        android:layout_marginTop="5dp"                        android:text="我的订单"                        android:textColor="#000000"                        android:textSize="12sp"                        android:textStyle="bold" />                    <!--导航-->                    <LinearLayout                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_marginTop="20dp"                        android:orientation="horizontal">                        <RelativeLayout                            android:id="@+id/fragment_user_btnBrowse"                            android:layout_width="0dp"                            android:layout_height="50dp"                            android:layout_weight="1">                            <ImageView                                android:id="@+id/fragment_user_BrowsImg"                                android:layout_width="35dp"                                android:layout_height="35dp"                                android:layout_centerHorizontal="true"                                android:src="@drawable/btn_browse" />                            <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                                android:layout_width="match_parent"                                android:layout_height="wrap_content"                                android:layout_alignParentBottom="true"                                android:gravity="center"                                android:text="交易记录"                                android:textColor="#000000"                                android:textSize="10sp" />                            <shlm.lmcs.com.lazycat.LazyShopView.PriceNumberView                                android:id="@+id/fragment_user_TransactionHow"                                android:layout_width="20dp"                                android:layout_height="20dp"                                android:layout_alignRight="@id/fragment_user_BrowsImg"                                android:background="@color/ThemeColor"                                android:gravity="center"                                android:padding="4dp"                                android:textColor="#ffffff"                                android:textSize="10sp"                                android:visibility="gone" />                        </RelativeLayout>                        <RelativeLayout                            android:id="@+id/fragment_user_btnSendSystem"                            android:layout_width="0dp"                            android:layout_height="50dp"                            android:layout_weight="1">                            <ImageView                                android:id="@+id/fragment_user_SendSystemImg"                                android:layout_width="32dp"                                android:layout_height="32dp"                                android:layout_centerHorizontal="true"                                android:layout_marginTop="2dp"                                android:src="@drawable/btn_order" />                            <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                                android:layout_width="match_parent"                                android:layout_height="wrap_content"                                android:layout_alignParentBottom="true"                                android:gravity="center"                                android:text="通知发货"                                android:textColor="#000000"                                android:textSize="10sp" />                            <shlm.lmcs.com.lazycat.LazyShopView.PriceNumberView                                android:id="@+id/fragment_user_TvSendSystemhow"                                android:layout_width="20dp"                                android:layout_height="20dp"                                android:layout_alignRight="@id/fragment_user_SendSystemImg"                                android:background="@color/ThemeColor"                                android:gravity="center"                                android:padding="4dp"                                android:textColor="#ffffff"                                android:textSize="10sp"                                android:visibility="gone" />                        </RelativeLayout>                        <RelativeLayout                            android:id="@+id/fragment_user_btnDelivery"                            android:layout_width="0dp"                            android:layout_height="50dp"                            android:layout_weight="1">                            <ImageView                                android:id="@+id/fragment_user_DeliveryImg"                                android:layout_width="32dp"                                android:layout_height="32dp"                                android:layout_centerHorizontal="true"                                android:src="@drawable/btn_delivery" />                            <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                                android:layout_width="match_parent"                                android:layout_height="wrap_content"                                android:layout_alignParentBottom="true"                                android:gravity="center"                                android:text="派送订单"                                android:textColor="#000000"                                android:textSize="10sp" />                            <shlm.lmcs.com.lazycat.LazyShopView.PriceNumberView                                android:id="@+id/fragment_user_TvDeliveryhow"                                android:layout_width="20dp"                                android:layout_height="20dp"                                android:layout_alignRight="@id/fragment_user_DeliveryImg"                                android:background="@color/ThemeColor"                                android:gravity="center"                                android:padding="4dp"                                android:textColor="#ffffff"                                android:textSize="10sp"                                android:visibility="gone" />                        </RelativeLayout>                    </LinearLayout>                    <LinearLayout                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_marginTop="20dp"                        android:orientation="horizontal">                        <RelativeLayout                            android:layout_width="0dp"                            android:layout_height="50dp"                            android:layout_weight="1">                            <ImageView                                android:layout_width="35dp"                                android:layout_height="35dp"                                android:layout_centerHorizontal="true"                                android:src="@drawable/btn_browse" />                            <TextView                                android:layout_width="match_parent"                                android:layout_height="wrap_content"                                android:layout_alignParentBottom="true"                                android:gravity="center"                                android:text="浏览记录"                                android:textColor="#000000"                                android:textSize="10sp" />                        </RelativeLayout>                        <RelativeLayout                            android:layout_width="0dp"                            android:layout_height="50dp"                            android:layout_weight="1">                            <ImageView                                android:layout_width="32dp"                                android:layout_height="32dp"                                android:layout_centerHorizontal="true"                                android:layout_marginTop="2dp"                                android:src="@drawable/btn_order" />                            <TextView                                android:layout_width="match_parent"                                android:layout_height="wrap_content"                                android:layout_alignParentBottom="true"                                android:gravity="center"                                android:text="订单记录"                                android:textColor="#000000"                                android:textSize="10sp" />                        </RelativeLayout>                        <RelativeLayout                            android:layout_width="0dp"                            android:layout_height="50dp"                            android:layout_weight="1">                            <ImageView                                android:layout_width="32dp"                                android:layout_height="32dp"                                android:layout_centerHorizontal="true"                                android:src="@drawable/btn_delivery" />                            <TextView                                android:layout_width="match_parent"                                android:layout_height="wrap_content"                                android:layout_alignParentBottom="true"                                android:gravity="center"                                android:text="派送查询"                                android:textColor="#000000"                                android:textSize="10sp" />                        </RelativeLayout>                    </LinearLayout>                </LinearLayout>                <!--其他功能-->                <LinearLayout                    android:id="@+id/fragment_user_otherNav"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginLeft="10dp"                    android:layout_marginTop="30dp"                    android:layout_marginRight="10dp"                    android:background="#ffffff"                    android:orientation="vertical"                    android:paddingBottom="15dp">                    <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginLeft="10dp"                        android:layout_marginTop="5dp"                        android:text="其他功能"                        android:textColor="#000000"                        android:textSize="12sp"                        android:textStyle="bold" />                    <!--导航-->                    <LinearLayout                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_marginTop="20dp"                        android:orientation="horizontal">                        <RelativeLayout                            android:id="@+id/fragment_userBtntoService"                            android:layout_width="0dp"                            android:layout_height="50dp"                            android:layout_weight="1">                            <ImageView                                android:layout_width="35dp"                                android:layout_height="35dp"                                android:layout_centerHorizontal="true"                                android:src="@drawable/ico_service" />                            <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                                android:layout_width="match_parent"                                android:layout_height="wrap_content"                                android:layout_alignParentBottom="true"                                android:gravity="center"                                android:text="帮助中心"                                android:textColor="#000000"                                android:textSize="10sp" />                        </RelativeLayout>                        <RelativeLayout                            android:id="@+id/fragment_user_btnAboutAs"                            android:layout_width="0dp"                            android:layout_height="50dp"                            android:layout_weight="1">                            <ImageView                                android:layout_width="32dp"                                android:layout_height="32dp"                                android:layout_centerHorizontal="true"                                android:layout_marginTop="2dp"                                android:src="@drawable/ico_about" />                            <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                                android:layout_width="match_parent"                                android:layout_height="wrap_content"                                android:layout_alignParentBottom="true"                                android:gravity="center"                                android:text="版本信息"                                android:textColor="#000000"                                android:textSize="10sp" />                        </RelativeLayout>                        <RelativeLayout                            android:id="@+id/fragment_user_btnTrack"                            android:layout_width="0dp"                            android:layout_height="50dp"                            android:layout_weight="1">                            <ImageView                                android:layout_width="32dp"                                android:layout_height="32dp"                                android:layout_centerHorizontal="true"                                android:src="@drawable/ico_footprint" />                            <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                                android:layout_width="match_parent"                                android:layout_height="wrap_content"                                android:layout_alignParentBottom="true"                                android:gravity="center"                                android:text="足迹"                                android:textColor="#000000"                                android:textSize="10sp" />                        </RelativeLayout>                    </LinearLayout>                </LinearLayout>            </LinearLayout>            <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginLeft="10dp"                android:layout_marginTop="15dp"                android:layout_marginRight="10dp"                android:layout_marginBottom="10dp"                android:text="@string/ProgramCopyright"                android:textColor="#020433"                android:textSize="10sp" />        </LinearLayout>    </ScrollView>    <!--功能维权-->    <LinearLayout        android:id="@+id/usercenterfrg_cardBody"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:orientation="vertical">    </LinearLayout></LinearLayout>