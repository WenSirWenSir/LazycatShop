<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:background="#ffffff"        android:orientation="horizontal">    <LinearLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:background="#ffffff"            android:orientation="horizontal">        <LinearLayout                android:layout_width="30dp"                android:layout_height="match_parent"                android:orientation="vertical">            <RelativeLayout                    android:layout_width="30dp"                    android:layout_height="wrap_content">                <View                        android:id="@+id/item_freight_msg_LineFreightin"                        android:layout_width="1dp"                        android:layout_height="fill_parent"                        android:layout_centerInParent="true"                        android:background="#efefef" />                <ImageView                        android:id="@+id/item_freight_msg_IcoFreightin"                        android:layout_width="25dp"                        android:layout_height="25dp"                        android:layout_centerHorizontal="true"                        android:layout_marginTop="15dp"                        android:src="@drawable/ico_freightspot" />            </RelativeLayout>        </LinearLayout>        <!--物流信息-->        <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="15dp"                android:layout_marginBottom="15dp"                android:orientation="vertical">            <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content">                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                        android:id="@+id/item_freight_msg_FreightMsg"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_marginRight="20dp"                        android:padding="5dp"                        android:text="【云仓库】 派送人:翁启鑫已经获取到商品信息,派单员电话:18063790393"                        android:textColor="#b7b4b8"                        android:textSize="13dp" />            </LinearLayout>            <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content">                <shlm.lmcs.com.lazycat.LazyShopView.PriceNumberView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginLeft="5dp"                        android:layout_marginTop="5dp"                        android:id="@+id/item_freight_msg_FreightTime"                        android:text="2019-10-23 19:00:73:22"                        android:textColor="#b7b4b8"                        android:textSize="10dp" />            </LinearLayout>        </LinearLayout>    </LinearLayout></LinearLayout>