<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="#ffffff"    android:fitsSystemWindows="true"    android:orientation="vertical">    <include layout="@layout/assembly_act_head" />    <ScrollView        android:layout_width="match_parent"        android:layout_height="match_parent">        <LinearLayout            android:layout_width="match_parent"            android:layout_height="match_parent"            android:orientation="vertical">            <!--APP ICO界面-->            <RelativeLayout                android:layout_width="match_parent"                android:layout_height="70dp"                android:layout_marginTop="50dp">                <LinearLayout                    android:id="@+id/activity_systemaboutus_icoBody"                    android:layout_width="70dp"                    android:layout_height="70dp"                    android:layout_centerHorizontal="true"                    android:background="@color/ThemeColor"                    android:orientation="horizontal">                </LinearLayout>                <TextView                    android:id="@+id/activity_systemaboutus_icoTitle"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_centerInParent="true"                    android:text="仓"                    android:textColor="#ffffff"                    android:textSize="30dp" />            </RelativeLayout>            <!--版本号-->            <RelativeLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="10dp">                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_centerInParent="true"                    android:text="版本号:1.0.7"                    android:textColor="#000000"                    android:textSize="13dp" />            </RelativeLayout>            <!--客服的电话-->            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="50dp"                android:orientation="vertical">                <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:orientation="horizontal">                    <RelativeLayout                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="1">                        <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_marginLeft="25dp"                            android:text="客服电话"                            android:textColor="#666666"                            android:textSize="15dp" />                    </RelativeLayout>                    <RelativeLayout                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="3">                        <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_alignParentRight="true"                            android:layout_marginRight="25dp"                            android:autoLink="phone"                            android:text="0597-3643965"                            android:textColor="@color/ThemeColor"                            android:textSize="15dp" />                    </RelativeLayout>                </LinearLayout>                <View                    android:layout_width="match_parent"                    android:layout_height="1dp"                    android:layout_marginLeft="25dp"                    android:layout_marginTop="12dp"                    android:background="#e9e9e9" />            </LinearLayout>            <!--工作日-->            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="10dp"                android:orientation="vertical">                <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:orientation="horizontal">                    <RelativeLayout                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="1">                        <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_marginLeft="25dp"                            android:text="工作日"                            android:textColor="#666666"                            android:textSize="15dp" />                    </RelativeLayout>                    <RelativeLayout                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="3">                        <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_alignParentRight="true"                            android:layout_marginRight="25dp"                            android:text="除双休日的8:00-18:00"                            android:textColor="#666666"                            android:textSize="15dp" />                    </RelativeLayout>                </LinearLayout>                <View                    android:layout_width="match_parent"                    android:layout_height="1dp"                    android:layout_marginLeft="25dp"                    android:layout_marginTop="12dp"                    android:background="#e9e9e9" />            </LinearLayout>            <!--客服邮箱-->            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="10dp"                android:orientation="vertical">                <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:orientation="horizontal">                    <RelativeLayout                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="1">                        <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_marginLeft="25dp"                            android:text="意见邮箱"                            android:textColor="#666666"                            android:textSize="15dp" />                    </RelativeLayout>                    <RelativeLayout                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="3">                        <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_alignParentRight="true"                            android:layout_marginRight="25dp"                            android:text="leftProgram@qq.com"                            android:textColor="#666666"                            android:textSize="15dp" />                    </RelativeLayout>                </LinearLayout>                <View                    android:layout_width="match_parent"                    android:layout_height="1dp"                    android:layout_marginLeft="25dp"                    android:layout_marginTop="12dp"                    android:background="#e9e9e9" />            </LinearLayout>            <!--物流反馈-->            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="10dp"                android:orientation="vertical">                <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:orientation="horizontal">                    <RelativeLayout                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="1">                        <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_marginLeft="25dp"                            android:text="版权所有"                            android:textColor="#666666"                            android:textSize="15dp" />                    </RelativeLayout>                    <RelativeLayout                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="3">                        <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_alignParentRight="true"                            android:layout_marginRight="25dp"                            android:text="龙岩市左边网络科技有限公司"                            android:textColor="@color/ThemeColor"                            android:textSize="15dp" />                    </RelativeLayout>                </LinearLayout>                <View                    android:layout_width="match_parent"                    android:layout_height="1dp"                    android:layout_marginLeft="25dp"                    android:layout_marginTop="12dp"                    android:background="#e9e9e9" />            </LinearLayout>        </LinearLayout>    </ScrollView></LinearLayout>