<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"        android:id="@+id/assembly_act_headBody"        android:layout_width="match_parent"        android:layout_height="40dp"        android:background="#efefef"        android:orientation="vertical">    <ImageView            android:id="@+id/assembly_act_headBackImg"            android:layout_width="20dp"            android:layout_height="20dp"            android:layout_centerVertical="true"            android:layout_marginLeft="5dp"            android:src="@drawable/ico_back" />    <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView            android:id="@+id/assembly_act_headTitle"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_centerInParent="true"            android:textColor="#08c299"            android:textSize="15sp"            android:textStyle="bold" /></RelativeLayout>