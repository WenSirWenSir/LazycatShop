<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:fitsSystemWindows="true"    android:orientation="vertical">    <include layout="@layout/assembly_act_head" />    <shlm.lmcs.com.lazycat.LazyCatProgramUnt.Views.RefreshScrollView        android:layout_width="match_parent"        android:layout_height="match_parent"        android:scrollbars="none"        >        <!--最外层的BODY-->        <LinearLayout            android:layout_width="match_parent"            android:layout_height="match_parent"            android:orientation="vertical">            <!--设置收件商家-->            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:orientation="vertical">                <!--店家的名称-->                <RelativeLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="20dp">                    <TextView                        android:id="@+id/assembly_confirmdeliverUsername"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_centerInParent="true"                        android:text="上杭金发日杂批发部"                        android:textColor="#08c299"                        android:textSize="17sp"                        android:textStyle="bold" />                    <TextView                        android:id="@+id/assembly_confirmdeliverIcoVip"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginLeft="5dp"                        android:layout_toRightOf="@id/assembly_confirmdeliverUsername"                        android:background="@color/colorVip"                        android:paddingLeft="4dp"                        android:paddingTop="1dp"                        android:paddingRight="4dp"                        android:paddingBottom="1dp"                        android:text="Vip"                        android:textColor="#ffffff"                        android:textSize="6dp" />                </RelativeLayout>                <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="25dp"                    android:orientation="horizontal">                    <RelativeLayout                        android:layout_width="0dp"                        android:layout_height="match_parent"                        android:layout_weight="1">                        <TextView                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_alignParentLeft="true"                            android:layout_centerVertical="true"                            android:layout_marginLeft="10dp"                            android:text="店铺负责人"                            android:textColor="#666666"                            android:textSize="11sp"                            android:textStyle="bold" />                        <TextView                            android:id="@+id/assembly_confirmdeliver_shopusePeople"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_alignParentRight="true"                            android:layout_centerVertical="true"                            android:layout_marginRight="10dp"                            android:text="翁启鑫"                            android:textColor="#000000"                            android:textSize="11sp" />                    </RelativeLayout>                    <RelativeLayout                        android:layout_width="0dp"                        android:layout_height="match_parent"                        android:layout_weight="1">                        <TextView                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_alignParentLeft="true"                            android:layout_centerVertical="true"                            android:layout_marginLeft="10dp"                            android:text="电话/手机"                            android:textColor="#666666"                            android:textSize="11sp"                            android:textStyle="bold" />                        <shlm.lmcs.com.lazycat.LazyShopView.PriceNumberView                            android:id="@+id/assembly_confirmdeliver_Shoptel"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_alignParentRight="true"                            android:layout_centerVertical="true"                            android:layout_marginRight="10dp"                            android:text="15206036936"                            android:textColor="#000000"                            android:textSize="11sp"                            android:textStyle="bold" />                    </RelativeLayout>                </LinearLayout>                <!--收件地址-->                <RelativeLayout                    android:layout_width="match_parent"                    android:layout_height="25dp">                    <TextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_alignParentLeft="true"                        android:layout_centerVertical="true"                        android:layout_marginLeft="10dp"                        android:text="收件地址"                        android:textColor="#666666"                        android:textSize="11sp"                        android:textStyle="bold" />                    <TextView                        android:id="@+id/assembly_confirmdeliver_Shopaddr"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_alignParentRight="true"                        android:layout_centerVertical="true"                        android:layout_marginRight="10dp"                        android:text="福建省龙岩市上杭县旧县乡梅西村12号"                        android:textColor="#000000"                        android:textSize="11sp" />                </RelativeLayout>            </LinearLayout>            <!--收货清单-->            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginLeft="10dp"                android:layout_marginTop="25dp"                android:text="收货清单列表"                android:textColor="#020433"                android:textSize="25sp"                android:textStyle="bold" />            <shlm.lmcs.com.lazycat.LazyShopView.PriceNumberView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginLeft="10dp"                android:layout_marginTop="10dp"                android:text="@string/toGetOrdershop"                android:textColor="#666666"                android:textSize="13sp"                />            <!--收货清单列表-->            <LinearLayout                android:layout_width="match_parent"                android:layout_height="match_parent"                android:layout_marginTop="10dp"                android:orientation="vertical">                <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="match_parent"                    android:layout_marginTop="10dp"                    android:orientation="vertical"                    android:id="@+id/activity_systemtopayorder_shoplistbody"                    >                </LinearLayout>                <View                    android:layout_width="match_parent"                    android:layout_height="2dp"                    android:background="@drawable/border_dotted_line"                    android:layerType="software" />                <RelativeLayout                    android:layout_width="match_parent"                    android:layout_height="50dp">                    <TextView                        android:layout_width="200dp"                        android:layout_height="35dp"                        android:layout_centerInParent="true"                        android:background="@color/ThemeColor"                        android:gravity="center"                        android:text="我已经清点完毕"                        android:textColor="#ffffff"                        android:textSize="12sp" />                </RelativeLayout>            </LinearLayout>        </LinearLayout>    </shlm.lmcs.com.lazycat.LazyCatProgramUnt.Views.RefreshScrollView></LinearLayout>