<?xml version="1.0" encoding="utf-8"?><!--提示信息--><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:orientation="vertical">    <RelativeLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_marginTop="10dp">        <!--仓库LOGO-->        <shlm.lmcs.com.lazycat.LazyShopView.ProgramCKTitle            android:id="@+id/assembly_dialog_message_log"            android:layout_width="30dp"            android:layout_height="30dp"            android:layout_centerVertical="true"            android:layout_marginLeft="15dp"            android:background="#f30d88"            android:gravity="center"            android:padding="5dp"            android:text="仓"            android:textColor="#ffffff"            android:textSize="15sp" />        <TextView            android:id="@+id/assembly_dialog_message_title"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_centerVertical="true"            android:layout_marginLeft="50dp"            android:text="提示信息"            android:textColor="#000000"            android:textSize="12sp" />    </RelativeLayout>    <View        android:layout_width="match_parent"        android:layout_height="1dp"        android:layout_marginLeft="10dp"        android:layout_marginTop="5dp"        android:layout_marginRight="10dp"        android:background="#000000" />    <TextView        android:id="@+id/assembly_dialog_message_content"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_marginLeft="10dp"        android:layout_marginTop="15dp"        android:layout_marginRight="10dp"        android:textColor="#000000"        android:textSize="13sp" />    <RelativeLayout        android:layout_width="match_parent"        android:layout_height="30dp"        android:layout_marginTop="30dp"        android:background="#ffffff">        <TextView            android:id="@+id/assembly_dialog_message_btnSure"            android:layout_width="30dp"            android:layout_height="wrap_content"            android:layout_alignParentRight="true"            android:layout_centerVertical="true"            android:layout_marginRight="10dp"            android:gravity="center"            android:text="确定"            android:textColor="#f30d88"            android:textSize="13sp" />        <TextView            android:id="@+id/assembly_dialog_message_btnCancale"            android:layout_width="30dp"            android:layout_height="wrap_content"            android:layout_centerVertical="true"            android:layout_marginRight="5dp"            android:layout_toLeftOf="@id/assembly_dialog_message_btnSure"            android:gravity="center"            android:text="取消"            android:textColor="#666666"            android:textSize="13sp" />    </RelativeLayout></LinearLayout>