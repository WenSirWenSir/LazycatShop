<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:fitsSystemWindows="true">    <shlm.lmcs.com.lazycat.LazyCatProgramUnt.Views.FragmentLR        android:id="@+id/activity_main_Framelayout"        android:layout_width="match_parent"        android:layout_height="match_parent"        android:layout_marginBottom="40dp">    </shlm.lmcs.com.lazycat.LazyCatProgramUnt.Views.FragmentLR>    <LinearLayout        android:layout_width="match_parent"        android:layout_height="40dp"        android:layout_alignParentBottom="true"        android:background="#efefef"        android:orientation="horizontal">        <RelativeLayout            android:id="@+id/activity_main_btn_IcoMain"            android:layout_width="0dp"            android:layout_height="40dp"            android:layout_weight="1">            <ImageView                android:layout_width="20dp"                android:layout_height="20dp"                android:layout_centerInParent="true"                android:scaleType="fitXY"                android:src="@drawable/btn_main" />            <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_alignParentBottom="true"                android:layout_centerHorizontal="true"                android:text="仓库首页"                android:textColor="#666666"                android:textSize="8sp" />        </RelativeLayout>        <RelativeLayout            android:id="@+id/activity_main_btnVip"            android:layout_width="0dp"            android:layout_height="40dp"            android:layout_weight="1">            <ImageView                android:id="@+id/activity_main_icoMessage"                android:layout_width="20dp"                android:layout_height="20dp"                android:layout_centerInParent="true"                android:scaleType="fitXY"                android:src="@drawable/btn_vip" />            <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_alignParentBottom="true"                android:layout_centerHorizontal="true"                android:paddingLeft="2dp"                android:text="加盟专享"                android:textColor="#666666"                android:textSize="8sp" />            <shlm.lmcs.com.lazycat.LazyShopView.PriceNumberView                android:id="@+id/activity_main_messageNumber"                android:layout_width="15dp"                android:layout_height="15dp"                android:layout_alignRight="@id/activity_main_icoMessage"                android:layout_centerHorizontal="true"                android:layout_marginLeft="15dp"                android:layout_marginTop="5dp"                android:background="#08c299"                android:gravity="center"                android:padding="1dp"                android:text="1"                android:textColor="#ffffff"                android:textSize="11sp"                android:visibility="gone" />        </RelativeLayout>        <RelativeLayout            android:id="@+id/activity_main_btnRecomd"            android:layout_width="0dp"            android:layout_height="40dp"            android:layout_weight="1">            <ImageView                android:layout_width="20dp"                android:layout_height="20dp"                android:layout_centerInParent="true"                android:scaleType="fitXY"                android:src="@drawable/btn_recomd" />            <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_alignParentBottom="true"                android:layout_centerHorizontal="true"                android:paddingLeft="2dp"                android:text="全部活动"                android:textColor="#666666"                android:textSize="8sp" />        </RelativeLayout>        <RelativeLayout            android:id="@+id/activity_main_btn_IcoUserCenter"            android:layout_width="0dp"            android:layout_height="40dp"            android:layout_weight="1">            <ImageView                android:layout_width="20dp"                android:layout_height="20dp"                android:layout_centerInParent="true"                android:scaleType="fitXY"                android:src="@drawable/btn_user" />            <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_alignParentBottom="true"                android:layout_centerHorizontal="true"                android:text="我的信息"                android:textColor="#666666"                android:textSize="8sp" />        </RelativeLayout>    </LinearLayout></RelativeLayout>