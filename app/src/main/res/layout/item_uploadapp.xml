<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="260dp"    android:layout_height="wrap_content">    <RelativeLayout        android:layout_width="260dp"        android:layout_height="wrap_content"        android:layout_centerInParent="true">        <LinearLayout            android:id="@+id/uploadapp_message"            android:layout_width="260dp"            android:layout_height="wrap_content"            android:background="#ffffff"            android:orientation="vertical">            <LinearLayout                android:layout_width="match_parent"                android:layout_height="100dp"                android:background="#08c299"                android:scaleType="fitXY" />            <TextView                android:layout_width="match_parent"                android:layout_height="30dp"                android:gravity="center"                android:text="新版本更新提示"                android:textColor="#020433"                android:textSize="15sp"                android:textStyle="bold" />            <shlm.lmcs.com.lazycat.LazyShopView.PriceNumberView                android:id="@+id/item_uploadappNewversion"                android:layout_width="match_parent"                android:layout_height="30dp"                android:layout_marginTop="20dp"                android:gravity="center|left"                android:paddingLeft="20dp"                android:textColor="#666666"                android:textSize="12sp" />            <shlm.lmcs.com.lazycat.LazyShopView.PriceNumberView                android:id="@+id/item_uploadappNewsize"                android:layout_width="match_parent"                android:layout_height="30dp"                android:gravity="center|left"                android:paddingLeft="20dp"                android:textColor="#666666"                android:textSize="12sp" />            <TextView                android:layout_width="match_parent"                android:layout_height="30dp"                android:layout_marginTop="5dp"                android:gravity="center|left"                android:paddingLeft="20dp"                android:text="新版本特性:"                android:textColor="#020433"                android:textSize="15sp"                android:textStyle="bold" />            <TextView                android:id="@+id/item_uploadappText"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:gravity="center|left"                android:paddingLeft="20dp"                android:textColor="#666666"                android:textSize="12sp" />            <LinearLayout                android:layout_width="match_parent"                android:layout_height="30dp"                android:layout_marginBottom="10dp"                android:gravity="center">                <TextView                    android:id="@+id/uploadapp_btnupload"                    android:layout_width="wrap_content"                    android:layout_height="25dp"                    android:background="@color/ThemeColor"                    android:gravity="center|left"                    android:paddingLeft="30dp"                    android:paddingRight="30dp"                    android:text="马上更新"                    android:textColor="#ffffff"                    android:textSize="10sp" />            </LinearLayout>        </LinearLayout>        <TextView            android:id="@+id/uploadapp_logo"            android:layout_width="100dp"            android:layout_height="100dp"            android:gravity="center"            android:text="仓"            android:textColor="#ffffff"            android:textSize="50dp" />    </RelativeLayout></RelativeLayout>