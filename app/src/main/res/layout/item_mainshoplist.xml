<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:background="#ffffff"    android:orientation="vertical">    <LinearLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_marginTop="10dp"        android:background="#ffffff"        android:orientation="horizontal">        <ImageView            android:id="@+id/item_mainshoplist_Shopimg"            android:layout_width="100dp"            android:layout_height="100dp" />        <LinearLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:orientation="vertical">            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content">                <shlm.lmcs.com.lazycat.LazyShopView.PriceNumberView                    android:id="@+id/item_mainshoplist_Title"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:ellipsize="end"                    android:maxLines="1"                    android:paddingLeft="15dp"                    android:paddingRight="15dp"                    android:textColor="#020433"                    android:textSize="12sp"                    android:textStyle="bold" />            </LinearLayout>            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="15dp"                android:orientation="horizontal"                android:paddingLeft="15dp">                <TextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:ellipsize="end"                    android:maxLines="1"                    android:text="@string/PriceSymbol"                    android:textColor="#020433"                    android:textSize="10sp"                    android:textStyle="bold" />                <shlm.lmcs.com.lazycat.LazyShopView.PriceNumberView                    android:id="@+id/item_mainshoplist_Tp"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:ellipsize="end"                    android:maxLines="1"                    android:textColor="#f30d65"                    android:textSize="17sp" />                <TextView                    android:id="@+id/item_mainshoplist_Company"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginLeft="2dp"                    android:ellipsize="end"                    android:maxLines="1"                    android:textColor="#020433"                    android:textSize="10sp" />                <TextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginLeft="5dp"                    android:ellipsize="end"                    android:maxLines="1"                    android:text="@string/PriceSymbol"                    android:textColor="#666666"                    android:textSize="8sp"                    android:textStyle="bold"                    android:visibility="gone" />                <shlm.lmcs.com.lazycat.LazyShopView.PriceNumberView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:ellipsize="end"                    android:maxLines="1"                    android:text="1788"                    android:textColor="#666666"                    android:textSize="15sp"                    android:visibility="gone" />            </LinearLayout>            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:background="#ffffff"                android:gravity="center_vertical"                android:orientation="horizontal"                android:paddingTop="10dp"                android:paddingBottom="5dp">                <ImageView                    android:layout_width="20dp"                    android:layout_height="20dp"                    android:layout_marginLeft="15dp"                    android:src="@drawable/ico_calendar" />                <shlm.lmcs.com.lazycat.LazyShopView.PriceNumberView                    android:id="@+id/item_mainshoplist_ExpAndPd"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:ellipsize="end"                    android:maxLines="1"                    android:textColor="#020433"                    android:textSize="10sp" />            </LinearLayout>            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="10dp"                android:background="#ffffff"                android:orientation="vertical">                <!--商家维护-->                <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:gravity="center"                    android:orientation="horizontal">                    <ImageView                        android:layout_width="17dp"                        android:layout_height="17dp"                        android:layout_marginLeft="15dp"                        android:src="@drawable/ico_shield" />                    <TextView                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_marginTop="1dp"                        android:text="【云仓库网络专送】"                        android:textColor="@color/colorText"                        android:textSize="10sp"                        android:textStyle="bold" />                </LinearLayout>            </LinearLayout>        </LinearLayout>    </LinearLayout>    <!--下滑虚线-->    <RelativeLayout        android:layout_width="match_parent"        android:layout_height="2dp"        android:layout_marginTop="5dp">        <View            android:layout_width="match_parent"            android:layout_height="2dp"            android:layout_centerInParent="true"            android:background="@drawable/border_dotted_line"            android:layerType="software" />    </RelativeLayout></LinearLayout>