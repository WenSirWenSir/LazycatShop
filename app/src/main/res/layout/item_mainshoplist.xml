<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:background="#ffffff"    android:orientation="vertical">    <LinearLayout        android:layout_width="match_parent"        android:layout_height="50dp"        android:layout_marginTop="10dp"        android:background="#ffffff"        android:orientation="horizontal">        <LinearLayout            android:layout_width="wrap_content"            android:layout_height="match_parent"            android:gravity="center_vertical">            <shlm.lmcs.com.lazycat.LazyCatProgramUnt.Views.CircleImageView                android:layout_width="45dp"                android:layout_height="45dp"                android:layout_centerVertical="true"                android:layout_marginLeft="15dp"                android:src="@drawable/a2343434" />        </LinearLayout>        <LinearLayout            android:layout_width="wrap_content"            android:layout_height="match_parent"            android:layout_marginLeft="10dp"            android:gravity="center_vertical"            android:orientation="vertical">            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content">                <ImageView                    android:layout_width="20dp"                    android:layout_height="20dp"                    android:layout_marginLeft="5dp"                    android:src="@drawable/ico_businessis" />                <TextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:text="上杭老林副食批发商行"                    android:textColor="#020433"                    android:textSize="10sp"                    android:textStyle="bold" />            </LinearLayout>            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="5dp"                android:gravity="center_vertical">                <ImageView                    android:layout_width="15dp"                    android:layout_height="15dp"                    android:layout_marginLeft="5dp"                    android:src="@drawable/ico_addr" />                <TextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:text="3.5KM"                    android:textColor="#020433"                    android:textSize="10sp"                    android:textStyle="bold" />                <TextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginLeft="5dp"                    android:text="-"                    android:textColor="#020433" />                <TextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginLeft="5dp"                    android:text="【仓库网络配送】预计1天到达"                    android:textColor="@color/ThemeColor"                    android:textSize="10sp"                    android:textStyle="bold" />            </LinearLayout>        </LinearLayout>        <RelativeLayout            android:layout_width="wrap_content"            android:layout_height="match_parent">            <ImageView                android:id="@+id/item_mainshoplist_btnLike"                android:layout_width="35dp"                android:layout_height="35dp"                android:layout_alignParentRight="true"                android:layout_centerVertical="true"                android:layout_marginRight="15dp"                android:src="@drawable/ico_nolike" />        </RelativeLayout>    </LinearLayout>    <LinearLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_marginTop="10dp"        android:background="#ffffff"        android:orientation="horizontal">        <ImageView            android:layout_width="100dp"            android:layout_height="100dp"            android:src="@drawable/_6" />        <LinearLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:orientation="vertical">            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content">                <TextView                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:ellipsize="end"                    android:maxLines="1"                    android:paddingLeft="15dp"                    android:paddingRight="15dp"                    android:text="友谊牌红枣袋装红枣儿童零食友谊牌红枣袋装红枣儿童零食友谊牌红枣袋装红枣儿童零食"                    android:textColor="#020433"                    android:textSize="12sp" />            </LinearLayout>            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="15dp"                android:orientation="horizontal"                android:paddingLeft="15dp">                <TextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:ellipsize="end"                    android:maxLines="1"                    android:text="@string/PriceSymbol"                    android:textColor="#020433"                    android:textSize="10sp" />                <shlm.lmcs.com.lazycat.LazyShopView.PriceNumberView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:ellipsize="end"                    android:maxLines="1"                    android:text="1788"                    android:textColor="#f30d65"                    android:textSize="17sp" />                <TextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginLeft="2dp"                    android:ellipsize="end"                    android:maxLines="1"                    android:text="/箱"                    android:textColor="#020433"                    android:textSize="10sp" />                <TextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginLeft="5dp"                    android:ellipsize="end"                    android:maxLines="1"                    android:text="@string/PriceSymbol"                    android:textColor="#666666"                    android:textSize="10sp" />                <shlm.lmcs.com.lazycat.LazyShopView.PriceNumberView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:ellipsize="end"                    android:maxLines="1"                    android:text="1788"                    android:textColor="#666666"                    android:textSize="15sp" />                <TextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginLeft="5dp"                    android:ellipsize="end"                    android:maxLines="1"                    android:text="每件x12包"                    android:textColor="#020433"                    android:textSize="11sp" />            </LinearLayout>            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:background="#ffffff"                android:orientation="horizontal"                android:paddingTop="5dp"                android:paddingBottom="5dp">                <shlm.lmcs.com.lazycat.LazyShopView.PriceNumberView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginLeft="15dp"                    android:ellipsize="end"                    android:maxLines="1"                    android:text="19年6月生产·24个月保质"                    android:textColor="#020433"                    android:textSize="10sp" />            </LinearLayout>            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:background="#ffffff"                android:orientation="vertical">                <!--商家维护-->                <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:gravity="center"                    android:orientation="horizontal">                    <ImageView                        android:layout_width="17dp"                        android:layout_height="17dp"                        android:layout_marginLeft="15dp"                        android:src="@drawable/ico_shield" />                    <TextView                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:text="仓库VIP特权【售后服务/超时未送达必赔/急速发货】"                        android:textColor="#666666"                        android:textSize="10sp"                        android:textStyle="bold" />                </LinearLayout>            </LinearLayout>        </LinearLayout>    </LinearLayout>    <!--下滑虚线-->    <RelativeLayout        android:layout_width="match_parent"        android:layout_height="2dp">        <View            android:layout_width="match_parent"            android:layout_height="2dp"            android:layout_centerInParent="true"            android:background="@drawable/border_dotted_line"            android:layerType="software" />    </RelativeLayout></LinearLayout>