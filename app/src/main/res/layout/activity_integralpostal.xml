<?xml version="1.0" encoding="utf-8"?><shlm.lmcs.com.lazycat.LazyCatProgramUnt.Views.RefreshScrollView xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:fitsSystemWindows="false"    android:scrollbars="none">    <LinearLayout        android:layout_width="match_parent"        android:layout_height="match_parent"        android:background="#efefef"        android:orientation="vertical">        <LinearLayout            android:layout_width="match_parent"            android:layout_height="250dp"            android:background="@color/ThemeColor"            android:orientation="vertical">            <RelativeLayout                android:layout_width="match_parent"                android:layout_height="match_parent">                <ImageView                    android:id="@+id/activity_integralpostalBtnBack"                    android:layout_width="25dp"                    android:layout_height="25dp"                    android:layout_marginLeft="5dp"                    android:layout_marginTop="30dp"                    android:src="@drawable/ico_back" />                <TextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_centerHorizontal="true"                    android:layout_marginTop="100dp"                    android:text="当前积分"                    android:textColor="#ffffff"                    android:textSize="15sp" />                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                    android:id="@+id/activity_integralpostalInteger"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_centerHorizontal="true"                    android:layout_marginTop="120dp"                    android:textColor="#ffffff"                    android:textSize="55sp" />            </RelativeLayout>        </LinearLayout>        <!--显示体现过的和可以使用的积分-->        <LinearLayout            android:layout_width="match_parent"            android:layout_height="60dp"            android:background="#ffffff">            <LinearLayout                android:layout_width="match_parent"                android:layout_height="match_parent"                android:layout_weight="1"                android:orientation="vertical">                <TextView                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="10dp"                    android:gravity="center|left"                    android:paddingLeft="5dp"                    android:text="能够兑换金额(元)"                    android:textColor="#666666"                    android:textSize="12sp"                    />                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                    android:id="@+id/activity_integralpostalTvuseIntegral"                    android:layout_width="match_parent"                    android:layout_height="match_parent"                    android:gravity="center|left"                    android:paddingLeft="5dp"                    android:textColor="#020433"                    android:textSize="12sp"                    />            </LinearLayout>            <LinearLayout                android:layout_width="match_parent"                android:layout_height="match_parent"                android:layout_weight="1"                android:orientation="vertical">                <TextView                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="10dp"                    android:gravity="center|left"                    android:paddingLeft="5dp"                    android:text="历史提现金额(元)"                    android:textColor="#666666"                    android:textSize="12sp" />                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                    android:layout_width="match_parent"                    android:layout_height="match_parent"                    android:gravity="center|left"                    android:paddingLeft="5dp"                    android:text="200.00"                    android:textColor="#020433"                    android:textSize="12sp"                    />            </LinearLayout>        </LinearLayout>        <!--显示总积分-->        <LinearLayout            android:layout_width="match_parent"            android:layout_height="50dp"            android:layout_marginTop="3dp"            android:background="#ffffff"            android:orientation="horizontal">            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginLeft="15dp"                android:text="总积分:"                android:textColor="#666666"                android:textSize="13dp" />            <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                android:id="@+id/activity_integralpostalTvtotalIntegral"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:gravity="center|left"                android:paddingLeft="5dp"                android:textColor="#666666"                android:textSize="25sp"                />        </LinearLayout>        <!--显示积分的使用-->        <LinearLayout            android:layout_width="match_parent"            android:layout_height="100dp"            android:layout_marginTop="10dp"            android:background="#ffffff"            android:paddingTop="10dp"            >            <LinearLayout                android:id="@+id/activity_integralpostalBtnDeposit"                android:layout_width="0dp"                android:layout_height="wrap_content"                android:layout_weight="1"                android:orientation="vertical">                <RelativeLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content">                    <ImageView                        android:layout_width="35dp"                        android:layout_height="35dp"                        android:layout_centerHorizontal="true"                        android:src="@drawable/ico_exchange" />                </RelativeLayout>                <TextView                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="5dp"                    android:gravity="center"                    android:text="兑换现金"                    android:textColor="#000000"                    android:textSize="13dp" />            </LinearLayout>            <LinearLayout                android:id="@+id/activity_integralpostalBtnDetiled"                android:layout_width="0dp"                android:layout_height="wrap_content"                android:layout_weight="1"                android:orientation="vertical">                <RelativeLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content">                    <ImageView                        android:layout_width="35dp"                        android:layout_height="35dp"                        android:layout_centerHorizontal="true"                        android:src="@drawable/ico_integer" />                </RelativeLayout>                <TextView                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="5dp"                    android:gravity="center"                    android:text="积分明细"                    android:textColor="#000000"                    android:textSize="13dp" />            </LinearLayout>            <LinearLayout                android:layout_width="0dp"                android:layout_height="wrap_content"                android:layout_weight="1"                android:orientation="vertical">                <RelativeLayout                    android:id="@+id/activity_integralpostalBtnBrand"                    android:layout_width="match_parent"                    android:layout_height="wrap_content">                    <ImageView                        android:layout_width="35dp"                        android:layout_height="35dp"                        android:layout_centerHorizontal="true"                        android:src="@drawable/ico_bankcard" />                </RelativeLayout>                <TextView                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="5dp"                    android:gravity="center"                    android:text="银行卡"                    android:textColor="#000000"                    android:textSize="13dp" />            </LinearLayout>        </LinearLayout>        <LinearLayout            android:layout_width="match_parent"            android:layout_height="100dp"            android:layout_marginTop="10dp"            android:background="#ffffff"            android:paddingTop="10dp"            >            <LinearLayout                android:id="@+id/activity_integralpostalBtnAddvip"                android:layout_width="0dp"                android:layout_height="wrap_content"                android:layout_weight="1"                android:orientation="vertical">                <RelativeLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content">                    <ImageView                        android:layout_width="35dp"                        android:layout_height="35dp"                        android:layout_centerHorizontal="true"                        android:src="@drawable/ico_joinme" />                </RelativeLayout>                <TextView                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="5dp"                    android:gravity="center"                    android:text="我要加盟"                    android:textColor="#000000"                    android:textSize="13dp" />            </LinearLayout>            <LinearLayout                android:id="@+id/activity_integralpostalBtnVipMsg"                android:layout_width="0dp"                android:layout_height="wrap_content"                android:layout_weight="1"                android:orientation="vertical">                <RelativeLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content">                    <ImageView                        android:layout_width="35dp"                        android:layout_height="35dp"                        android:layout_centerHorizontal="true"                        android:src="@drawable/ico_joingoodfor" />                </RelativeLayout>                <TextView                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="5dp"                    android:gravity="center"                    android:text="加盟特权"                    android:textColor="#000000"                    android:textSize="13dp" />            </LinearLayout>            <LinearLayout                android:id="@+id/activity_integralpostalBtnInvest"                android:layout_width="0dp"                android:layout_height="wrap_content"                android:layout_weight="1"                android:orientation="vertical">                <RelativeLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content">                    <ImageView                        android:layout_width="35dp"                        android:layout_height="35dp"                        android:layout_centerHorizontal="true"                        android:src="@drawable/ico_investment" />                </RelativeLayout>                <TextView                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="5dp"                    android:gravity="center"                    android:text="积分投资"                    android:textColor="#000000"                    android:textSize="13dp" />            </LinearLayout>        </LinearLayout>    </LinearLayout></shlm.lmcs.com.lazycat.LazyCatProgramUnt.Views.RefreshScrollView>