<?xml version="1.0" encoding="utf-8"?><shlm.lmcs.com.lazycat.LazyCatProgramUnt.Views.RefreshScrollView xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:fitsSystemWindows="true"    android:scrollbars="none">    <LinearLayout        android:layout_width="match_parent"        android:layout_height="match_parent"        android:orientation="vertical">        <include layout="@layout/assembly_act_head" />        <RelativeLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_marginTop="10dp">            <LinearLayout                android:layout_width="match_parent"                android:layout_height="170dp"                android:layout_centerHorizontal="true"                android:layout_marginLeft="10dp"                android:layout_marginRight="10dp"                android:background="@drawable/border_bandcard"                android:orientation="vertical">                <RelativeLayout                    android:layout_width="match_parent"                    android:layout_height="match_parent">                    <TextView                        android:id="@+id/activity_addbandcard_TextbankName"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginLeft="15dp"                        android:layout_marginTop="10dp"                        android:text="****"                        android:textColor="#ffffff"                        android:textSize="15sp" />                    <shlm.lmcs.com.lazycat.LazyShopView.PriceNumberView                        android:id="@+id/activity_addbandcard_number"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_centerInParent="true"                        android:text="****************"                        android:textColor="#ffffff"                        android:textSize="25sp" />                    <TextView                        android:id="@+id/activity_addbandcard_cardPeople"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_alignParentRight="true"                        android:layout_alignParentBottom="true"                        android:layout_marginRight="15dp"                        android:layout_marginBottom="10dp"                        android:text="持卡人:***"                        android:textColor="#ffffff"                        android:textSize="15sp" />                </RelativeLayout>            </LinearLayout>        </RelativeLayout>        <LinearLayout            android:layout_width="match_parent"            android:layout_height="match_parent"            android:orientation="vertical"            android:visibility="visible"            android:id="@+id/activity_addbandcard_inputBody"            >            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="10dp"                android:background="#efefef"                android:paddingLeft="10dp"                android:paddingTop="10dp"                android:paddingBottom="10dp">                <TextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:text="选择银行"                    android:textColor="#666666"                    android:textSize="13sp" />            </LinearLayout>            <Spinner                android:id="@+id/activity_addbandcard_bankName"                android:layout_width="200dp"                android:layout_height="30dp"                android:layout_marginLeft="10dp"                android:layout_marginTop="5dp"                android:gravity="center|left"                android:inputType="number"                android:textSize="15sp" />            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="10dp"                android:background="#efefef"                android:paddingLeft="10dp"                android:paddingTop="10dp"                android:paddingBottom="10dp">                <TextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:text="卡号"                    android:textColor="#666666"                    android:textSize="13sp" />            </LinearLayout>            <EditText                android:id="@+id/activity_addbandcard_cardNumber"                android:layout_width="match_parent"                android:layout_height="30dp"                android:layout_marginLeft="10dp"                android:layout_marginTop="5dp"                android:background="@null"                android:gravity="center|left"                android:inputType="number"                android:textSize="15sp" />            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="10dp"                android:background="#efefef"                android:paddingLeft="10dp"                android:paddingTop="10dp"                android:paddingBottom="10dp">                <TextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:text="持卡人"                    android:textColor="#666666"                    android:textSize="13sp" />            </LinearLayout>            <EditText                android:id="@+id/activity_addbandcard_cardSavePeople"                android:layout_width="match_parent"                android:layout_height="30dp"                android:layout_marginLeft="10dp"                android:layout_marginTop="5dp"                android:background="@null"                android:gravity="center|left"                android:inputType="text"                android:textSize="15sp" />            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="10dp"                android:background="#efefef"                android:paddingLeft="10dp"                android:paddingTop="10dp"                android:paddingBottom="10dp">                <TextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:text="银行预留电话"                    android:textColor="#666666"                    android:textSize="13sp" />            </LinearLayout>            <EditText                android:id="@+id/activity_addbandcard_cardSaveTel"                android:layout_width="match_parent"                android:layout_height="30dp"                android:layout_marginLeft="10dp"                android:layout_marginTop="5dp"                android:background="@null"                android:gravity="center|left"                android:inputType="number"                android:textSize="15sp" />        </LinearLayout>        <LinearLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_marginTop="10dp"            android:background="#efefef"            android:paddingLeft="10dp"            android:paddingTop="10dp"            android:paddingBottom="10dp">            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:text="温馨提示"                android:textColor="#666666"                android:textSize="13sp" />        </LinearLayout>        <!--备注信息-->        <TextView            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_marginLeft="10dp"            android:layout_marginTop="5dp"            android:layout_marginRight="10dp"            android:layout_marginBottom="10dp"            android:text="@string/addBankCardMsg"            android:textSize="12sp" />        <RelativeLayout            android:layout_width="match_parent"            android:layout_height="50dp">            <TextView                android:id="@+id/activity_systemaddtoVipBtn_to"                android:layout_width="200dp"                android:layout_height="35dp"                android:layout_centerInParent="true"                android:background="@drawable/border_bandcard"                android:gravity="center"                android:text="绑定该银行卡"                android:textColor="#ffffff"                android:textSize="15sp" />        </RelativeLayout>    </LinearLayout></shlm.lmcs.com.lazycat.LazyCatProgramUnt.Views.RefreshScrollView>