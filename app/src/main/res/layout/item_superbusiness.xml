<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:background="#efefef"        android:orientation="vertical">    <LinearLayout            android:id="@+id/item_superbusinessBody"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_margin="10dp"            android:orientation="vertical"            android:paddingLeft="5dp"            android:paddingTop="10dp"            android:paddingRight="5dp"            android:paddingBottom="10dp">        <RelativeLayout                android:layout_width="match_parent"                android:layout_height="wrap_content">            <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                    android:id="@+id/item_superbusinessBueinssname"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_alignParentLeft="true"                    android:layout_marginLeft="10dp"                    android:textColor="#000000"                    android:textSize="17dp" />        </RelativeLayout>        <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="5dp"                android:orientation="horizontal">            <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                    android:id="@+id/item_superbusinessTel"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_alignParentLeft="true"                    android:layout_marginLeft="10dp"                    android:textColor="#000000"                    android:textSize="10dp" />        </LinearLayout>        <RelativeLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="15dp">            <LinearLayout                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_alignParentLeft="true"                    android:orientation="horizontal">                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_centerHorizontal="true"                        android:layout_marginLeft="10dp"                        android:background="@color/ThemeColor"                        android:padding="3dp"                        android:text="等待取货数量:"                        android:textColor="#ffffff"                        android:textSize="10dp" />                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                        android:id="@+id/item_superbusinessPayhow"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:paddingRight="10dp"                        android:layout_centerHorizontal="true"                        android:background="@color/colorReserve"                        android:padding="3dp"                        android:textColor="#ffffff"                        android:textSize="10dp" />            </LinearLayout>        </RelativeLayout>        <RelativeLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="15dp">            <LinearLayout                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_alignParentLeft="true"                    android:orientation="horizontal">                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_centerHorizontal="true"                        android:layout_marginLeft="10dp"                        android:background="@color/ThemeColor"                        android:padding="3dp"                        android:text="合计货物价值:"                        android:textColor="#ffffff"                        android:textSize="10dp" />                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                        android:id="@+id/item_superbusinessTotal"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_centerHorizontal="true"                        android:paddingRight="10dp"                        android:background="@color/colorReserve"                        android:padding="3dp"                        android:textColor="#ffffff"                        android:textSize="10dp" />            </LinearLayout>        </RelativeLayout>    </LinearLayout></LinearLayout>