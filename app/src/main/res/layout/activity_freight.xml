<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"        xmlns:app="http://schemas.android.com/apk/res-auto"        android:layout_width="match_parent"        android:layout_height="match_parent"        android:fitsSystemWindows="true"        android:orientation="vertical">    <include layout="@layout/assembly_act_head" />    <View            android:layout_width="match_parent"            android:layout_height="1dp"            android:layout_marginTop="5dp"            android:layout_marginBottom="5dp"            android:background="#efefef" />    <LinearLayout            android:layout_width="match_parent"            android:layout_height="wrap_content">        <RelativeLayout                android:layout_width="120dp"                android:layout_height="match_parent">            <com.makeramen.roundedimageview.RoundedImageView                    android:id="@+id/activity_freightShopimg"                    android:layout_width="100dp"                    android:layout_height="100dp"                    android:layout_centerInParent="true"                    app:riv_corner_radius="5dp"                    app:riv_oval="false" />        </RelativeLayout>        <LinearLayout                android:layout_width="0dp"                android:layout_height="wrap_content"                android:layout_weight="3"                android:orientation="vertical">            <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="5dp"                    android:layout_marginBottom="5dp"                    android:orientation="horizontal">                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:text="物流状态:"                        android:textColor="#b8b7b4"                        android:textSize="14dp" />                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                        android:id="@+id/activity_freightFreightStatus"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginLeft="5dp"                        android:textColor="#08c299"                        android:textSize="14dp" />            </LinearLayout>            <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="5dp"                    android:layout_marginBottom="5dp"                    android:orientation="horizontal">                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:text="快递公司:"                        android:textColor="#b8b7b4"                        android:textSize="14dp" />                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                        android:id="@+id/activity_freightFreightBusiness"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginLeft="5dp"                        android:text="云仓库配送"                        android:textColor="#020433"                        android:textSize="14dp" />            </LinearLayout>            <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="5dp"                    android:layout_marginBottom="5dp"                    android:orientation="horizontal">                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:text="运单编号:"                        android:textColor="#b8b7b4"                        android:textSize="14dp" />                <shlm.lmcs.com.lazycat.LazyShopView.PriceNumberView                        android:id="@+id/activity_freightFreightOrdernumber"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginLeft="5dp"                        android:text="98388577338"                        android:textColor="#020433"                        android:textSize="14dp" />            </LinearLayout>            <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="5dp"                    android:layout_marginBottom="5dp"                    android:orientation="horizontal">                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:text="服务电话:"                        android:textColor="#b8b7b4"                        android:textSize="14dp" />                <shlm.lmcs.com.lazycat.LazyShopView.PriceNumberView                        android:id="@+id/activity_freightFreightTel"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginLeft="5dp"                        android:text="0597-3566997"                        android:textColor="#020433"                        android:textSize="14dp" />            </LinearLayout>        </LinearLayout>    </LinearLayout>    <LinearLayout            android:layout_width="match_parent"            android:layout_height="15dp"            android:layout_marginTop="15dp"            android:background="#efefef"></LinearLayout>    <ListView            android:id="@+id/activity_freight_listView"            android:layout_width="match_parent"            android:layout_height="match_parent"            android:divider="@null"            android:scrollbars="none">    </ListView></LinearLayout>