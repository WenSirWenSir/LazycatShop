<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:fitsSystemWindows="true"    android:background="#efefef"    android:orientation="vertical">    <include layout="@layout/assembly_act_head" />    <!--设置银行卡-->    <LinearLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_margin="10dp"        android:id="@+id/activity_integraldepositBody"        android:background="#ffffff"        android:orientation="vertical">        <LinearLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:orientation="horizontal"            android:layout_marginTop="25dp"            >            <LinearLayout                android:layout_width="match_parent"                android:layout_height="match_parent"                android:layout_weight="2">                <TextView                    android:layout_width="match_parent"                    android:layout_height="30dp"                    android:gravity="center"                    android:text="到账银行卡:"                    android:textColor="#666666"                    android:textSize="13sp" />            </LinearLayout>            <LinearLayout                android:layout_width="match_parent"                android:layout_height="match_parent"                android:layout_weight="1"                android:orientation="vertical">                <TextView                    android:layout_width="match_parent"                    android:layout_height="30dp"                    android:gravity="center"                    android:id="@+id/activity_integraldepositBanksAndCard"                    android:textColor="#000000"                    android:textSize="13sp" />                <TextView                    android:layout_width="match_parent"                    android:layout_height="30dp"                    android:gravity="center"                    android:text="预计2个工作日内到账"                    android:textColor="#666666"                    android:textSize="10sp" />            </LinearLayout>        </LinearLayout>        <RelativeLayout            android:layout_width="match_parent"            android:layout_height="wrap_content">            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginLeft="20dp"                android:layout_marginTop="10dp"                android:textColor="#000000"                android:text="提现金额"                android:textSize="10sp" />        </RelativeLayout>        <!--提现的输入框-->        <LinearLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_marginTop="10dp"            >            <!--提现的金额的符号-->            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:textColor="#000000"                android:layout_marginLeft="20dp"                android:textSize="25sp"                android:textStyle="bold"                android:text="@string/PriceSymbol"                />            <!--提现的金额-->            <EditText                android:layout_width="match_parent"                android:layout_height="30dp"                android:background="@null"                android:textSize="22sp"                android:id="@+id/activity_integraldepositEditMoney"                android:textStyle="bold"                android:inputType="number"                android:textColor="@color/colorText"                />        </LinearLayout>        <!--下划线-->        <View            android:layout_width="match_parent"            android:layout_height="2dp"            android:layerType="software"            android:layout_marginTop="10dp"            android:background="@drawable/border_dotted_line"            android:layout_marginLeft="15dp"            android:layout_marginRight="15dp"            />        <!--确定提现按钮-->        <RelativeLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_marginTop="35dp"            android:layout_marginBottom="15dp"            >            <TextView                android:layout_width="120dp"                android:layout_height="30dp"                android:gravity="center"                android:text="确定提现"                android:layout_centerHorizontal="true"                android:textColor="#ffffff"                android:id="@+id/activity_integraldepositBtnDeposit"                android:background="#08c299"                />                    </RelativeLayout>    </LinearLayout></LinearLayout>