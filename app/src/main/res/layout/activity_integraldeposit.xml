<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"        android:layout_width="match_parent"        android:layout_height="match_parent"        android:background="#efefef"        android:fitsSystemWindows="true"        android:orientation="vertical">    <include layout="@layout/assembly_act_head" />    <!--设置银行卡-->    <LinearLayout            android:id="@+id/activity_integraldepositBody"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_margin="10dp"            android:background="#ffffff"            android:orientation="vertical">        <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="25dp"                android:orientation="horizontal">            <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="match_parent"                    android:layout_weight="2">                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                        android:layout_width="match_parent"                        android:layout_height="30dp"                        android:gravity="center"                        android:text="到账银行卡:"                        android:textColor="#666666"                        android:textSize="13dp" />            </LinearLayout>            <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="match_parent"                    android:layout_weight="1"                    android:orientation="vertical">                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                        android:id="@+id/activity_integraldepositBanksAndCard"                        android:layout_width="match_parent"                        android:layout_height="30dp"                        android:gravity="center"                        android:textColor="#000000"                        android:textSize="13dp" />                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                        android:layout_width="match_parent"                        android:layout_height="30dp"                        android:gravity="center"                        android:text="预计2个工作日内到账"                        android:textColor="#666666"                        android:textSize="10dp" />            </LinearLayout>        </LinearLayout>        <RelativeLayout                android:layout_width="match_parent"                android:layout_height="wrap_content">            <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginLeft="20dp"                    android:layout_marginTop="10dp"                    android:text="提现金额"                    android:textColor="#000000"                    android:textSize="10dp" />        </RelativeLayout>        <!--提现的输入框-->        <RelativeLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="10dp">            <!--提现的金额的符号-->            <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                    android:id="@+id/activity_integraldepositSymbol"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_centerVertical="true"                    android:layout_marginLeft="20dp"                    android:text="@string/PriceSymbol"                    android:textColor="#000000"                    android:textSize="15dp"                    android:textStyle="bold" />            <!--提现的金额-->            <EditText                    android:id="@+id/activity_integraldepositEditMoney"                    android:layout_width="150dp"                    android:layout_height="30dp"                    android:layout_centerVertical="true"                    android:layout_toRightOf="@id/activity_integraldepositSymbol"                    android:background="@null"                    android:inputType="number"                    android:maxLength="5"                    android:textColor="@color/colorText"                    android:textSize="25dp"                    android:textStyle="bold" />        </RelativeLayout>        <!--下划线-->        <View                android:layout_width="match_parent"                android:layout_height="2dp"                android:layout_marginLeft="15dp"                android:layout_marginTop="10dp"                android:layout_marginRight="15dp"                android:background="@drawable/border_dotted_line"                android:layerType="software" />        <!--超出提现的金额-->        <RelativeLayout                android:layout_width="match_parent"                android:layout_height="wrap_content">            <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                    android:id="@+id/activity_integraldepositTomuchmsg"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_alignParentLeft="true"                    android:paddingLeft="15dp"                    android:paddingTop="8dp"                    android:paddingBottom="8dp"                    android:textColor="@color/colorText"                    android:textSize="10dp" />            <ImageView                    android:id="@+id/activity_integraldepositBtnAsk"                    android:layout_width="18dp"                    android:layout_height="18dp"                    android:layout_centerVertical="true"                    android:layout_marginLeft="5dp"                    android:layout_toRightOf="@id/activity_integraldepositTomuchmsg"                    android:src="@drawable/ico_ask" />        </RelativeLayout>        <!--确定提现按钮-->        <RelativeLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="35dp"                android:layout_marginBottom="15dp">            <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                    android:id="@+id/activity_integraldepositBtnDeposit"                    android:layout_width="120dp"                    android:layout_height="30dp"                    android:layout_centerHorizontal="true"                    android:background="#08c299"                    android:gravity="center"                    android:text="确定提现"                    android:textColor="#ffffff" />        </RelativeLayout>    </LinearLayout></LinearLayout>