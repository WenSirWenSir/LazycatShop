l="true"                        android:layout_marginLeft="15dp"                        android:text="店铺地址"                        android:textColor="#ffffff"                        android:textSize="12dp" />            </RelativeLayout>        </LinearLayout>        <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginLeft="1dp"                android:layout_marginRight="1dp">            <RelativeLayout                    android:layout_width="match_parent"                    android:layout_height="25dp"                    android:background="#ffffff">                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                        android:id="@+id/activity_SystemadminVipShopaddr"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_centerVertical="true"                        android:layout_marginLeft="15dp"                        android:text="-"                        android:textColor="#020433"                        android:textSize="12dp" />            </RelativeLayout>        </LinearLayout>        <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginLeft="1dp"                android:layout_marginRight="1dp">            <RelativeLayout                    android:layout_width="match_parent"                    android:layout_height="25dp"                    android:background="@color/colorVip">                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_centerVertical="true"                        android:layout_marginLeft="15dp"                 