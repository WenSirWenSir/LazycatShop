<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"        android:layout_width="match_parent"        android:layout_height="match_parent"        android:background="@color/colorVip"        android:fitsSystemWindows="true"        android:orientation="vertical">    <ImageView            android:layout_width="20dp"            android:layout_height="20dp"            android:layout_margin="15dp"            android:src="@drawable/ico_back2" />    <ImageView            android:id="@+id/activity_SystemadminVipImg"            android:layout_width="60dp"            android:layout_height="60dp"            android:layout_centerHorizontal="true"            android:layout_marginTop="100dp"            android:src="@drawable/ico_admin" />    <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_below="@id/activity_SystemadminVipImg"            android:layout_centerHorizontal="true"            android:text="加盟授权管理"            android:textSize="12dp" />    <LinearLayout            android:id="@+id/activity_systemAdminVipUserBody"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_centerHorizontal="true"            android:layout_marginLeft="20dp"            android:layout_marginTop="200dp"            android:layout_marginRight="20dp"            android:orientation="vertical">        <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginLeft="1dp"                android:layout_marginTop="3dp"                android:layout_marginRight="1dp">            <RelativeLayout                    android:layout_width="match_parent"                    android:layout_height="25dp">                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_centerVertical="true"                        android:layout_marginLeft="15dp"                        android:text="店铺名称"                        android:textColor="#ffffff"                        android:textSize="12dp" />            </RelativeLayout>        </LinearLayout>        <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginLeft="1dp"                android:layout_marginRight="1dp">            <RelativeLayout                    android:layout_width="match_parent"                    android:layout_height="25dp"                    android:background="#ffffff">                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                        android:id="@+id/activity_SystemadminVipShopName"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_centerVertical="true"                        android:layout_marginLeft="15dp"                        android:text="-"                        android:textColor="#020433"                        android:textSize="12dp" />            </RelativeLayout>        </LinearLayout>        <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginLeft="1dp"                android:layout_marginRight="1dp">            <RelativeLayout                    android:layout_width="match_parent"                    android:layout_height="25dp">                <shlm.lmcs.com.lazycat.LazyShopView.SystemTextView                        android:layout_width="wrap_content"                        android:layo