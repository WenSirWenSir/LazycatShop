<?xml version="1.0" encoding="utf-8"?><!--显示商品的信息 在搜索中--><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="130dp"    android:background="#ffffff"    android:fitsSystemWindows="false"    android:orientation="vertical">    <RelativeLayout        android:layout_width="match_parent"        android:layout_height="match_parent">        <LinearLayout            android:layout_width="match_parent"            android:layout_height="match_parent">            <!--图片地址-->            <RelativeLayout                android:layout_width="130dp"                android:layout_height="130dp"                android:layout_weight="0">                <ImageView                    android:id="@+id/item_searchshoplist_img"                    android:layout_width="120dp"                    android:layout_height="120dp"                    android:layout_centerInParent="true" />            </RelativeLayout>            <LinearLayout                android:layout_width="match_parent"                android:layout_height="match_parent"                android:layout_weight="1"                android:orientation="vertical">                <!--标题-->                <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="5dp"                    android:gravity="center_vertical">                    <TextView                        android:id="@+id/item_searchshoplist_titleStatic"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:paddingLeft="3dp"                        android:paddingTop="1dp"                        android:paddingRight="3dp"                        android:paddingBottom="1dp"                        android:textSize="10sp" />                    <TextView                        android:id="@+id/item_searchshoplist_title"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_marginRight="5dp"                        android:ellipsize="end"                        android:maxLines="1"                        android:paddingLeft="5dp"                        android:textColor="#000000"                        android:textSize="12sp"                        android:textStyle="bold" />                </LinearLayout>                <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="30dp"                    android:layout_marginTop="10dp"                    android:orientation="horizontal">                    <shlm.lmcs.com.lazycat.LazyShopView.PriceNumberView                        android:id="@+id/item_searchshoplist_priceSymbol"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:text="@string/PriceSymbol"                        android:textColor="#020433"                        android:textSize="10sp"                        android:textStyle="bold" />                    <shlm.lmcs.com.lazycat.LazyShopView.PriceNumberView                        android:id="@+id/item_searchshoplist_price"                        android:layout_width="wrap_content"                        android:layout_height="match_parent"                        android:layout_marginLeft="2dp"                        android:textSize="20sp" />                    <shlm.lmcs.com.lazycat.LazyShopView.PriceNumberView                        android:id="@+id/item_searchshoplist_company"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginLeft="2dp"                        android:textColor="#020433"                        android:textSize="10sp"                        android:textStyle="bold" />                    <shlm.lmcs.com.lazycat.LazyShopView.PriceNumberView                        android:id="@+id/item_searchshoplist_dottedlinePrice"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginLeft="2dp"                        android:textSize="10sp"                        android:visibility="visible" />                </LinearLayout>                <!--商家信息-->                <RelativeLayout                    android:layout_width="match_parent"                    android:layout_height="20dp"                    android:layout_marginTop="5dp">                    <ImageView                        android:layout_width="20dp"                        android:layout_height="20dp"                        android:layout_centerVertical="true"                        android:src="@drawable/ico_deliver_cart" />                    <TextView                        android:id="@+id/item_searchshoplist_businessName"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:text="云仓库专送"                        android:layout_centerVertical="true"                        android:layout_marginLeft="25dp"                        android:textStyle="bold"                        android:textColor="#020433"                        android:textSize="10sp" />                </RelativeLayout>                <TextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginTop="5dp"                    android:textSize="10sp"                    android:paddingLeft="8dp"                    android:paddingRight="8dp"                    android:paddingBottom="1dp"                    android:id="@+id/item_searchshoplist_deliverVip"                    android:paddingTop="1dp"                    android:layout_marginLeft="5dp"                    android:textColor="#ffffff"                    android:background="@color/colornoVip"                    />                <!--商家名称-->                <LinearLayout                    android:id="@+id/item_searchshoplist_businessBody"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginTop="5dp"                    android:layout_toRightOf="@id/activity_showshopoffice_businessName"                    android:gravity="center_vertical">                    <ImageView                        android:layout_width="15dp"                        android:layout_height="15dp"                        android:src="@drawable/ico_addr" />                    <TextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:text="距离你"                        android:textStyle="bold"                        android:textColor="@color/colorText"                        android:textSize="10sp"                        />                    <TextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:id="@+id/item_searchshoplist_distance"                        android:textColor="#08c299"                        android:textSize="10sp"                        android:textStyle="bold" />                    <TextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginLeft="2dp"                        android:text="km"                        android:textColor="#020433"                        android:textSize="10sp"                        android:textStyle="bold" />                    <TextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginLeft="5dp"                        android:text="-"                        android:textColor="#020433" />                    <TextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginLeft="5dp"                        android:id="@+id/item_searchshoplist_arrivalTimeTitle_Begin"                        android:text="预计"                        android:textColor="#020433"                        android:textSize="10sp"                        android:textStyle="bold" />                    <TextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginLeft="2dp"                        android:layout_marginRight="2dp"                        android:id="@+id/item_searchshoplist_arrivalTime"                        android:textColor="#08c299"                        android:textSize="10sp"                        android:textStyle="bold" />                    <TextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:id="@+id/item_searchshoplist_arrivalTimeTitle_End"                        android:text="送达"                        android:textColor="#020433"                        android:textSize="10sp"                        android:textStyle="bold" />                </LinearLayout>                <!--显示一些其他的信息-->                <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content">                </LinearLayout>            </LinearLayout>        </LinearLayout>    </RelativeLayout></LinearLayout>