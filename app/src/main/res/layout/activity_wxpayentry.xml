<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:fitsSystemWindows="true"    android:orientation="vertical">    <RelativeLayout        android:layout_width="match_parent"        android:layout_height="35dp"        android:background="@color/colorPaywindowBack">        <TextView            android:id="@+id/activity_wxpayentry_finish"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentRight="true"            android:layout_centerVertical="true"            android:layout_marginRight="15dp"            android:text="完成"            android:textColor="#ffffff"            android:textSize="13sp" />    </RelativeLayout>    <shlm.lmcs.com.lazycat.LazyCatProgramUnt.Views.RefreshScrollView        android:layout_width="match_parent"        android:layout_height="match_parent"        android:background="#efefef">        <LinearLayout            android:layout_width="match_parent"            android:layout_height="match_parent"            android:background="#efefef"            android:orientation="vertical">            <LinearLayout                android:layout_width="match_parent"                android:layout_height="200dp"                android:background="@color/colorPaywindowBack"                android:orientation="vertical">                <RelativeLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_centerHorizontal="true"                    android:layout_marginTop="30dp"                    android:orientation="vertical"                    android:paddingLeft="15dp">                    <TextView                        android:id="@+id/activity_wxpayentry_payStatus"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_centerHorizontal="true"                        android:text="支付完成"                        android:textColor="#ffffff"                        android:textSize="20sp" />                </RelativeLayout>                <RelativeLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_centerHorizontal="true"                    android:orientation="vertical"                    android:paddingLeft="15dp">                    <ImageView                        android:id="@+id/activity_wxpayentry_payStatusImg"                        android:layout_width="100dp"                        android:layout_height="100dp"                        android:layout_centerHorizontal="true"                        android:src="@drawable/ico_payok"                        android:textColor="#ffffff" />                </RelativeLayout>                <TextView                    android:id="@+id/activity_wxpayentry_payMsg"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="5dp"                    android:gravity="center"                    android:textColor="#ffffff"                    android:textSize="12sp" />            </LinearLayout>            <!--显示支付详情-->            <LinearLayout                android:layout_width="match_parent"                android:layout_height="match_parent"                android:background="#e9e9e9"                android:orientation="vertical">                <LinearLayout                    android:id="@+id/activity_wxpayentry_Msgbody"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginLeft="5dp"                    android:layout_marginRight="5dp"                    android:orientation="vertical">                    <LinearLayout                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:orientation="horizontal">                        <TextView                            android:layout_width="wrap_content"                            android:layout_height="45dp"                            android:layout_marginTop="25dp"                            android:background="#ffffff"                            android:gravity="center|right"                            android:paddingLeft="25dp"                            android:text="实付金额:"                            android:textColor="#666666"                            android:textSize="15sp" />                        <shlm.lmcs.com.lazycat.LazyShopView.PriceNumberView                            android:layout_width="0dp"                            android:layout_height="45dp"                            android:layout_marginTop="25dp"                            android:layout_weight="1"                            android:background="#ffffff"                            android:gravity="center_vertical"                            android:paddingLeft="25dp"                            android:text="68.00元"                            android:textColor="#666666"                            android:textSize="15sp" />                    </LinearLayout>                    <LinearLayout                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:orientation="horizontal">                        <TextView                            android:layout_width="wrap_content"                            android:layout_height="45dp"                            android:background="#ffffff"                            android:gravity="center|right"                            android:paddingLeft="25dp"                            android:text="支付方式:"                            android:textColor="#666666"                            android:textSize="15sp" />                        <TextView                            android:layout_width="0dp"                            android:layout_height="45dp"                            android:layout_weight="1"                            android:background="#ffffff"                            android:gravity="center_vertical"                            android:paddingLeft="25dp"                            android:text="微信支付"                            android:textColor="#666666"                            android:textSize="15sp" />                    </LinearLayout>                    <LinearLayout                        android:layout_width="match_parent"                        android:layout_height="45dp"                        android:background="#ffffff"                        android:orientation="horizontal">                        <TextView                            android:layout_width="wrap_content"                            android:layout_height="45dp"                            android:background="#ffffff"                            android:gravity="center|right"                            android:paddingLeft="25dp"                            android:text="充值账户:"                            android:textColor="#666666"                            android:textSize="15sp" />                        <shlm.lmcs.com.lazycat.LazyShopView.PriceNumberView                            android:id="@+id/activity_wxpayentry_toAccount"                            android:layout_width="0dp"                            android:layout_height="45dp"                            android:layout_weight="1"                            android:background="#ffffff"                            android:gravity="center_vertical"                            android:paddingLeft="25dp"                            android:textColor="#666666"                            android:textSize="15sp" />                    </LinearLayout>                    <LinearLayout                        android:layout_width="match_parent"                        android:layout_height="45dp"                        android:background="#ffffff"                        android:orientation="horizontal">                        <TextView                            android:layout_width="wrap_content"                            android:layout_height="45dp"                            android:background="#ffffff"                            android:gravity="center|right"                            android:paddingLeft="25dp"                            android:text="充值用途:"                            android:textColor="#666666"                            android:textSize="15sp" />                        <TextView                            android:layout_width="0dp"                            android:layout_height="45dp"                            android:layout_weight="1"                            android:background="#ffffff"                            android:gravity="center_vertical"                            android:paddingLeft="25dp"                            android:text="云仓库加盟Vip"                            android:textColor="#666666"                            android:textSize="15sp" />                    </LinearLayout>                </LinearLayout>            </LinearLayout>            <!--用户已经知晓-->            <RelativeLayout                android:layout_width="match_parent"                android:layout_height="match_parent"                android:layout_marginTop="10dp"                android:background="#efefef">                <TextView                    android:id="@+id/activity_wxpayentry_btnUserknow"                    android:layout_width="200dp"                    android:layout_height="35dp"                    android:layout_centerHorizontal="true"                    android:layout_marginTop="10dp"                    android:gravity="center"                    android:text="我已知晓"                    android:textColor="#ffffff" />            </RelativeLayout>        </LinearLayout>    </shlm.lmcs.com.lazycat.LazyCatProgramUnt.Views.RefreshScrollView></LinearLayout>