<resources>    <!-- Base application theme. -->    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">        <!-- Customize your theme here. -->        <item name="colorPrimary">@color/colorPrimary</item>        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>        <item name="colorAccent">@color/colorAccent</item>    </style>    <!--弹窗动画-->    <style name="DialogStyle" parent="android:Animation">        <!--进入的动画-->        <item name="android:windowEnterAnimation">@anim/android_anim_big_to_smail</item>        <!--退出的动画-->        <item name="android:windowExitAnimation">@anim/android_anim_smail_to_big</item>    </style></resources>