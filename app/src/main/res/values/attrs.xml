<?xml version="1.0" encoding="utf-8"?><resources>    <declare-styleable name="mArcViewpage">        <attr name="arcHeight" format="integer" /><!--控件的高度-->        <attr name="arcColor" format="color" /><!--控件的颜色-->    </declare-styleable>    <declare-styleable name="FilletImageView">        <attr name="FilletHeight" format="integer" /><!--控件的高度-->        <attr name="FilletColor" format="color" /><!--控件的颜色-->        <attr name="is_circle" format="boolean" /><!--是否为圆形-->        <attr name="corner_top_left_radius" format="dimension" /><!--左上角半径-->        <attr name="corner_top_right_radius" format="dimension" /><!--右上角半径-->        <attr name="corner_bottom_left_radius" format="dimension" /><!--左下角半径-->        <attr name="corner_bottom_right_radius" format="dimension" /><!--右下角半径-->        <attr name="corner_radius" format="dimension" /><!--统一设置四个角的半径-->        <attr name="border_width" format="dimension" /><!--边框的宽度-->        <attr name="border_color" format="color" /><!--边框的颜色-->        <attr name="inner_border_width" format="dimension" /><!--内层边框-->        <attr name="inner_border_color" format="color" /><!--内层边框的颜色-->        <attr name="is_cover_src" format="color" /><!--是否覆盖图片内容-->        <attr name="mask_color" format="color" /><!--遮罩层-->    </declare-styleable></resources>